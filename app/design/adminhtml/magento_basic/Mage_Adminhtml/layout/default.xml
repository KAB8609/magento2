<layout version="0.1.0">
    <default>
        <reference name="head">
            <block type="Mage_Page_Block_Html_Head_Css" name="reset-css">
                <arguments>
                    <file>reset.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="boxes-css">
                <arguments>
                    <file>boxes.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="custom-css">
                <arguments>
                    <file>custom.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="iestyles-css">
                <arguments>
                    <file>iestyles.css</file>
                    <properties>
                        <attributes/>
                        <ie_condition>lt IE 8</ie_condition>
                    </properties>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="below-ie-css">
                <arguments>
                    <file>below_ie7.css</file>
                    <properties>
                        <attributes/>
                        <ie_condition>lt IE 7</ie_condition>
                    </properties>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="ie-css">
                <arguments>
                    <file>ie7.css</file>
                    <properties>
                        <attributes/>
                        <ie_condition>IE 7</ie_condition>
                    </properties>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="menu-css">
                <arguments>
                    <file>menu.css</file>
                    <properties>
                        <attributes>media="screen, projection"</attributes>
                    </properties>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="print-css">
                <arguments>
                    <file>print.css</file>
                    <properties>
                        <attributes>media="print"</attributes>
                    </properties>
                </arguments>
            </block>
        </reference>
    </default>
</layout>
