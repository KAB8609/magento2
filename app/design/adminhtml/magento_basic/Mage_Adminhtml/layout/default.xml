<layout version="0.1.0">
    <default>
        <reference name="head">
            <action method="addCss"><file>reset.css</file></action>
            <action method="addCss"><file>boxes.css</file></action>
            <action method="addCss"><file>custom.css</file></action>
            <action method="addCss"><file>iestyles.css</file><params/><if>lt IE 8</if></action>
            <action method="addCss"><file>below_ie7.css</file><params/><if>lt IE 7</if></action>
            <action method="addCss"><file>ie7.css</file><params/><if>IE 7</if></action>
            <action method="addCss"><file>menu.css</file><params>media="screen, projection"</params></action>
            <action method="addCss"><file>print.css</file><params>media="print"</params></action>
        </reference>
    </default>
</layout>
