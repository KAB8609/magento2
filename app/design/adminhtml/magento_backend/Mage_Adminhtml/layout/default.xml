<layout version="0.1.0">
    <default>
        <reference name="head">
            <block type="Mage_Page_Block_Html_Head_Css" name="mui-reset-css">
                <arguments>
                    <file>mui/reset.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="mui-base-css">
                <arguments>
                    <file>mui/base.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="mui-elements-css">
                <arguments>
                    <file>mui/elements.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="mui-form-css">
                <arguments>
                    <file>mui/form.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="mui-components-css">
                <arguments>
                    <file>mui/components.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="css-admin-css">
                <arguments>
                    <file>css/admin.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="jquery-farbtastic-css-farbtastic-css">
                <arguments>
                    <file>jquery/farbtastic/css/farbtastic.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="css-header-css">
                <arguments>
                    <file>css/header.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="css-pages-css">
                <arguments>
                    <file>css/pages.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="mui-utils-css">
                <arguments>
                    <file>mui/utils.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="mage-core-prototype-magento-css">
                <arguments>
                    <file>Mage_Core::prototype/magento.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="css-debug-css">
                <arguments>
                    <file>css/debug.css</file>
                </arguments>
            </block>
            <!-- temporary to debug -->
            <block type="Mage_Page_Block_Html_Head_Css" name="mui-print-css">
                <arguments>
                    <file>mui/print.css</file>
                    <properties>
                        <attributes>media="print"</attributes>
                    </properties>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Script" name="lib-modernizr-js">
                <arguments>
                    <file>lib/modernizr.js</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Script" name="js-head-js">
                <arguments>
                    <file>js/head.js</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Script" name="mage-dropdown-js">
                <arguments>
                    <file>mage/dropdown.js</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Script" name="jquery-jquery-tabs-js">
                <arguments>
                    <file>jquery/jquery.tabs.js</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Script" name="jquery-farbtastic-jquery-farbtastic-js">
                <arguments>
                    <file>jquery/farbtastic/jquery.farbtastic.js</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Script" name="js-theme-js">
                <arguments>
                    <file>js/theme.js</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Script" name="js-jquery-details-js">
                <arguments>
                    <file>js/jquery.details.js</file>
                </arguments>
            </block>
        </reference>
    </default>
</layout>
