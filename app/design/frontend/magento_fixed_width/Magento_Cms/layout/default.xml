<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <reference name="header">
        <block class="Magento\Cms\Block\Block" name="header.callout" as="headerCallout">
            <action method="setBlockId">
                <argument name="block_id" xsi:type="string">header_callout</argument>
            </action>
        </block>
    </reference>
    <move element="cms_footer_links" destination="footer" after="footer_links"/>
</layout>
