<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <update handle="page_one_column"/>
    <referenceBlock name="head">
        <block class="Magento\Page\Block\Html\Head\Script" name="jquery-jqzoom-js-jquery-jqzoom-core-pack-js">
            <arguments>
                <argument name="file" xsi:type="string">jquery/jqzoom/js/jquery.jqzoom-core-pack.js</argument>
            </arguments>
        </block>
        <block class="Magento\Page\Block\Html\Head\Css" name="jquery-jqzoom-css-jquery-jqzoom-css">
            <arguments>
                <argument name="file" xsi:type="string">jquery/jqzoom/css/jquery.jqzoom.css</argument>
            </arguments>
        </block>
    </referenceBlock>
    <remove name="magento-catalog-zoom-css"/>
</layout>
