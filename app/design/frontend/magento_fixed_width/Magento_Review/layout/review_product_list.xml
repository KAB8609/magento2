<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="review_product_list">
        <update handle="page_one_column"/>
        <reference name="head">
            <action method="removeItem">
                <type>css</type>
                <name>Magento_Catalog::zoom.css</name>
            </action>
            <action method="addJs">
                <file>jquery/jqzoom/js/jquery.jqzoom-core-pack.js</file>
            </action>
            <action method="addCss">
                <file>jquery/jqzoom/css/jquery.jqzoom.css</file>
            </action>
        </reference>
    </handle>
</layout>
