<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <block class="Magento\Page\Block\Html" name="root" output="1" template="1column.phtml">
        <container name="after_body_start" as="after_body_start" label="Page Top"/>
        <block class="Magento\Page\Block\Html\Head" name="head" as="head"/>
        <block class="Magento\View\Element\Messages" name="global_messages" as="global_messages"/>
        <block class="Magento\View\Element\Messages" name="messages" as="messages"/>
        <container name="content" as="content" label="Main Content Area">
            <container name="cms.wrapper" label="CMS Content Wrapper" htmlTag="div" htmlClass="std">
                <block class="Magento\WebsiteRestriction\Block\Cms\Stub" name="cms_page"/>
            </container>
        </container>
        <block class="Magento\Page\Block\Html\Footer" name="footer" as="footer" template="html/footer.phtml"/>
    </block>
</layout>
