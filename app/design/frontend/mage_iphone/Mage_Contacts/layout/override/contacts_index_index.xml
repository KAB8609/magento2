<layout version="0.1.0">
    <contacts_index_index translate="label" type="page" parent="default">
        <label>Contact Us Form</label>
        <reference name="head">
            <action method="setTitle" translate="title" module="Mage_Contacts">
                <title>Contact Us</title>
            </action>
        </reference>
        <reference name="root">
            <action method="setTemplate">
                <template>1column.phtml</template>
            </action>
            <action method="setHeaderTitle" translate="title" module="Mage_Contacts">
                <title>Contact Us</title>
            </action>
        </reference>
        <reference name="content">
            <block type="Mage_Core_Block_Template" name="contactForm" template="Mage_Contacts::form.phtml"/>
        </reference>
    </contacts_index_index>
</layout>
