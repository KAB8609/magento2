<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <reference name="head">
        <block class="Magento\Page\Block\Html\Head\Script" name="magento-catalogsearch-form-mini-js">
            <arguments>
                <argument name="file" xsi:type="string">Magento_CatalogSearch::form-mini.js</argument>
            </arguments>
        </block>
    </reference>
    <reference name="header">
        <block class="Magento\Core\Block\Template" name="top.search" as="topSearch" template="Magento_CatalogSearch::form.mini.phtml"/>
    </reference>
</layout>
