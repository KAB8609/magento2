<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="default">
        <reference name="head">
            <action method="addJS">
                <argument name="file" xsi:type="string">Magento_CatalogSearch::form-mini.js</argument>
            </action>
        </reference>
        <reference name="header">
            <block class="Magento_Core_Block_Template" name="top.search" as="topSearch" template="Magento_CatalogSearch::form.mini.phtml"/>
        </reference>
    </handle>
</layout>
