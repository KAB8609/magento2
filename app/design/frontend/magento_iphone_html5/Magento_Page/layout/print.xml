<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="print">
        <reference name="head">
            <action method="addCss">
                <file>css/styles.css</file>
            </action>
            <action method="addCss">
                <file>css/styles-ie.css</file>
                <params/>
                <if>lt IE 8</if>
            </action>
            <action method="addCss">
                <file>css/widgets.css</file>
            </action>
            <action method="addCss">
                <file>css/print.css</file>
                <params>media="print"</params>
            </action>
            <action method="addJs">
                <file>lib/ds-sleight.js</file>
                <params/>
                <if>lt IE 7</if>
            </action>
        </reference>
    </handle>
</layout>
