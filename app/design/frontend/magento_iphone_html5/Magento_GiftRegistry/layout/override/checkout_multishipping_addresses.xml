<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="checkout_multishipping_addresses">
        <reference name="content">
            <block class="Magento\GiftRegistry\Block\Customer\Checkout" name="giftregistry.customer.checkout" template="customer/checkout/multishipping.phtml"/>
        </reference>
        <reference name="additional.product.info">
            <block class="Magento\GiftRegistry\Block\Cart\Product\Mark" name="giftregistry.product.mark" template="cart/product/mark.phtml"/>
        </reference>
    </handle>
</layout>
