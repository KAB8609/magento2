<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="rss_catalog_new">
        <block class="Magento_Rss_Block_Catalog_New" output="1" name="rss.catalog.new">
            <action method="addPriceBlockType">
                <argument name="type" xsi:type="string">msrp_rss</argument>
                <argument name="block" xsi:type="string">Magento_Catalog_Block_Product_Price</argument>
                <argument name="template" xsi:type="string">product/price_msrp_rss.phtml</argument>
            </action>
        </block>
    </handle>
</layout>
