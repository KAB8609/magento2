<config>
    <modules>
        <Saas_Sales>
            <version>2.0.0.0</version>
            <active>false</active>
            <depends>
                <Mage_Backend />
                <Mage_Sales />
            </depends>
        </Saas_Sales>
    </modules>
    <global>
        <events>
            <adminhtml_controller_action_predispatch_start>
                <observers>
                    <disable_adminhtml_sales_transactions_controller>
                        <class>Saas_Sales_Model_Observer</class>
                        <method>disableAdminhtmlSalesTransactionsController</method>
                    </disable_adminhtml_sales_transactions_controller>
                </observers>
            </adminhtml_controller_action_predispatch_start>
        </events>
    </global>
</config>
