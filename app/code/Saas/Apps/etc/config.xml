<config>
    <modules>
        <Saas_Apps>
            <version>2.0.0.0</version>
            <active>false</active>
            <depends>
                <Mage_Backend/>
                <Mage_Core/>
                <Mage_Adminhtml/>
                <Saas_Catalog/>
            </depends>
        </Saas_Apps>
    </modules>
    <default>
        <app_tab_url>
            <en_US>https://golinks.magento.com/apps/admin</en_US>
            <fr_FR>https://golinks.magento.com/apps/admin</fr_FR>
            <en_GB>https://golinks.magento.com/uk/app-tab</en_GB>
        </app_tab_url>
    </default>
    <admin>
        <routers>
            <adminhtml>
                <use>admin</use>
                <args>
                    <modules>
                        <Saas_Apps before="Mage_Adminhtml">Saas_Apps</Saas_Apps>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
    </adminhtml>
</config>
