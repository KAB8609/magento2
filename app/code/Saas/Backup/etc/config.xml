<config>
    <modules>
        <Saas_Backup>
            <version>2.0.0.0</version>
            <active>false</active>
            <depends>
                <Mage_Backup/>
                <Mage_Backend/>
                <Saas_Saas/>
            </depends>
        </Saas_Backup>
    </modules>
    <global>
        <events>
            <controller_action_predispatch_adminhtml>
                <observers>
                    <limit_backup_functionality>
                        <class>Saas_Backup_Model_Observer</class>
                        <method>limitBackupFunctionality</method>
                    </limit_backup_functionality>
                </observers>
            </controller_action_predispatch_adminhtml>
        </events>
    </global>
</config>
