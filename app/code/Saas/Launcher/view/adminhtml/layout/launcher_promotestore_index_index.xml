<layout version="0.1.0">
    <launcher_promotestore_index_index>
        <update handle="promotestore_index"/>
        <reference name="head">
            <block type="Mage_Page_Block_Html_Head_Css" name="saas-launcher-page-launcher-css">
                <arguments>
                    <file>Saas_Launcher::page/launcher.css</file>
                </arguments>
            </block>
        </reference>
        <reference name="js">
            <block type="Mage_Backend_Block_Template" template="Saas_Launcher::page/js.phtml" name="page.js"/>
        </reference>
        <reference name="content">
            <block type="Saas_Launcher_Block_Adminhtml_Promotestore_Page" name="promotestore.page" template="page/promotestore/promotestore.phtml"/>
        </reference>
    </launcher_promotestore_index_index>
</layout>
