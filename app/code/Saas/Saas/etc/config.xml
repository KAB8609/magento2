<config>
    <modules>
        <Saas_Saas>
            <version>2.0.0.0</version>
            <active>false</active>
        </Saas_Saas>
    </modules>
    <global>
        <events>
            <controller_action_predispatch>
                <observers>
                    <saas_limit_functionality>
                        <class>Saas_Saas_Model_Limitation_Observer</class>
                        <method>limitFunctionality</method>
                    </saas_limit_functionality>
                </observers>
            </controller_action_predispatch>
        </events>
        <di>
            <preferences>
                <Saas_Saas_Model_Limitation_SpecificationInterface>Saas_Saas_Model_Limitation_Specification_Composite</Saas_Saas_Model_Limitation_SpecificationInterface>
            </preferences>
            <Saas_Saas_Model_Limitation_Specification_Composite>
                <parameters>
                    <specificationFactory>Saas_Saas_Model_Limitation_Specification_Factory</specificationFactory>
                </parameters>
            </Saas_Saas_Model_Limitation_Specification_Composite>
        </di>
    </global>
    <adminhtml>
        <di>
            <preferences>
                <Mage_Backend_Model_Config_Structure_Converter>Saas_Saas_Model_DisabledConfiguration_Structure_Converter_Filter</Mage_Backend_Model_Config_Structure_Converter>
            </preferences>
        </di>
        <events>
            <core_config_data_save_before>
                <observers>
                    <saas_saas>
                        <class>Saas_Saas_Model_DisabledConfiguration_Observer</class>
                        <method>checkConfigSaveAllowed</method>
                    </saas_saas>
                </observers>
            </core_config_data_save_before>
        </events>
    </adminhtml>
    <default>
        <!-- Start of overridden values -->
        <dev>
            <js>
                <merge_files>1</merge_files>
                <minify>1</minify>
            </js>
        </dev>
        <!-- End of overridden values -->
    </default>
</config>
