<config>
    <acl>
        <resources>
            <resource id="Mage_Adminhtml::admin">
                <resource id="Mage_Adminhtml::system">
                    <resource id="Enterprise_Logging::enterprise_logging" module="Enterprise_Logging" title="Action Log" sortOrder="70">
                        <resource id="Enterprise_Logging::enterprise_logging_events" title="Report" module="Enterprise_Logging" sortOrder="10"/>
                        <resource id="Enterprise_Logging::backups" title="Archive" module="Enterprise_Logging" sortOrder="20" />
                    </resource>
                </resource>
            </resource>

            <resource id="Old_Mage_Adminhtml::admin">
                <resource id="Old_Mage_Adminhtml::system">
                    <resource id="Old_Mage_Adminhtml::config">
                        <resource id="Old_Enterprise_Logging::logging" module="Enterprise_Logging" title="Logging" />
                    </resource>
                    <resource id="Old_Enterprise_Logging::enterprise_logging" module="Enterprise_Logging" title="Admin Logging">
                        <resource id="Old_Enterprise_Logging::enterprise_logging_events" title="Report" />
                        <resource id="Old_Enterprise_Logging::backups" title="Archive" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
