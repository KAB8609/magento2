<config>
    <modules>
        <Enterprise_Queue>
            <version>2.0.0.0</version>
            <active>false</active>
            <system_requirements>
                <php>
                    <extensions>
                        <gearman />
                    </extensions>
                </php>
            </system_requirements>
        </Enterprise_Queue>
    </modules>
    <global>
        <queue>
            <adapter>
                <gearman>
                    <title>Gearman</title>
                    <servers>127.0.0.1:4730</servers>
                </gearman>
            </adapter>
        </queue>
        <resources>
            <enterprise_queue_setup>
                <setup>
                    <module>Enterprise_Queue</module>
                    <class>Magento_Core_Model_Resource_Setup</class>
                </setup>
            </enterprise_queue_setup>
        </resources>
    </global>
</config>
