<layout version="0.1.0">
    <enterprise_rma_return_view_handle translate="label" type="page" parent="enterprise_rma_return_create_handle">
        <label>RMA Return View Image</label>
        <reference name="content">
            <block name="enterprise_rma_item_form_template" type="Enterprise_Eav_Block_Form_Template">
                <action method="addRenderer">
                    <type>image</type>
                    <renderer_block>Enterprise_Rma_Block_Form_Renderer_Image</renderer_block>
                    <template>form/view/image.phtml</template>
                </action>
            </block>
        </reference>
    </enterprise_rma_return_view_handle>
</layout>
