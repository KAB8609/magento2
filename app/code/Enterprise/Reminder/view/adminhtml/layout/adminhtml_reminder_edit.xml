<layout version="0.1.0">
    <adminhtml_reminder_edit>
        <reference name="head">
            <block type="Mage_Page_Block_Html_Head_Script" name="mage-adminhtml-wysiwyg-widget-js">
                <arguments>
                    <file>mage/adminhtml/wysiwyg/widget.js</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="prototype-windows-themes-default-css">
                <arguments>
                    <file>prototype/windows/themes/default.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="mage-core-prototype-magento-css">
                <arguments>
                    <file>Mage_Core::prototype/magento.css</file>
                </arguments>
            </block>
        </reference>
        <reference name="content">
            <block type="Enterprise_Reminder_Block_Adminhtml_Reminder_Edit" name="adminhtml_reminder_edit"/>
        </reference>
        <reference name="left">
            <block type="Enterprise_Reminder_Block_Adminhtml_Reminder_Edit_Tabs" name="adminhtml_reminder_edit_tabs"/>
        </reference>
    </adminhtml_reminder_edit>
</layout>
