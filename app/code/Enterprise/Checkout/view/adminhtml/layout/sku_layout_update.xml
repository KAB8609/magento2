<layout version="0.1.0">
    <sku_layout_update>
        <update handle="adminhtml_sku_errors_block"/>
        <reference name="head">
            <block type="Mage_Page_Block_Html_Head_Script" name="mage-adminhtml-catalog-product-composite-configure-js">
                <arguments>
                    <file>Mage_Adminhtml::catalog/product/composite/configure.js</file>
                </arguments>
            </block>
        </reference>
        <reference name="data">
            <block type="Enterprise_Checkout_Block_Adminhtml_Sales_Order_Create_Sku" name="additional_area" template="Mage_Adminhtml::sales/order/create/abstract.phtml">
                <block type="Enterprise_Checkout_Block_Adminhtml_Sales_Order_Create_Sku_Add"/>
            </block>
            <block type="Enterprise_Checkout_Block_Adminhtml_Sales_Order_Create_Sku_Errors" name="errors"/>
        </reference>
    </sku_layout_update>
</layout>
