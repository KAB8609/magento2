<layout version="0.1.0">
    <default>
        <reference name="head">
            <block type="Mage_Page_Block_Html_Head_Script" name="enterprise-catalogevent-js-ticker-js" ifconfig="catalog/enterprise_catalogevent/enabled">
                <arguments>
                    <file>Enterprise_CatalogEvent::js/ticker.js</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Css" name="enterprise-catalogevent-widgets-css" ifconfig="catalog/enterprise_catalogevent/enabled">
                <arguments>
                    <file>Enterprise_CatalogEvent::widgets.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Script" name="enterprise-catalogevent-js-carousel-js" ifconfig="catalog/enterprise_catalogevent/enabled">
                <arguments>
                    <file>Enterprise_CatalogEvent::js/carousel.js</file>
                </arguments>
            </block>
        </reference>
    </default>
</layout>
