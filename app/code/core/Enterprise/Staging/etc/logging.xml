<logging>
  <actions>
    <staging_manage_edit>
      <event>staging</event>
      <action>view</action>
    </staging_manage_edit>

    <staging_manage_save>
      <event>staging</event>
      <action>save</action>
      <model>enterprise_staging/staging</model>
      <handler>enterprise_staging/observer::loggingstagingsaveafter</handler>
      <allow_model_repeat>1</allow_model_repeat>
    </staging_manage_save>

    <staging_manage_mergePost>
      <event>staging</event>
      <action>mergePost</action>
      <handler>enterprise_staging/observer::loggingmergeafter</handler>
    </staging_manage_mergePost>

    <staging_backup_rollbackPost>
      <event>staging</event>
      <action>rollback</action>
      <handler>enterprise_staging/observer::loggingrollbackafter</handler>
    </staging_backup_rollbackPost>

    <staging_backup_backupDelete>
      <event>staging</event>
      <action>delete</action>
    </staging_backup_backupDelete>
  </actions>

  <labels>
      <list translate="staging">
          <staging>Staging</staging>
      </list>
  </labels>
</logging>
