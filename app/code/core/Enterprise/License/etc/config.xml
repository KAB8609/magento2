<config>
    <modules>
        <Enterprise_License>
            <version>1.11.0.0</version>
        </Enterprise_License>
    </modules>
    <adminhtml>
        <translate>
            <modules>
                <Enterprise_License>
                    <files>
                        <default>Enterprise_License.csv</default>
                    </files>
                </Enterprise_License>
            </modules>
        </translate>
        <layout>
            <updates>
                <enterprise_license module="Enterprise_License">
                    <file>layout.xml</file>
                </enterprise_license>
            </updates>
        </layout>
        <events>
            <admin_user_authenticate_after>
                <observers>
                    <enterprise_license>
                        <class>Enterprise_License_Model_Observer</class>
                        <method>adminUserAuthenticateAfter</method>
                    </enterprise_license>
                </observers>
            </admin_user_authenticate_after>
            <controller_action_predispatch>
                <observers>
                    <enterprise_license>
                        <class>Enterprise_License_Model_Observer</class>
                        <method>preDispatch</method>
                    </enterprise_license>
                </observers>
            </controller_action_predispatch>
        </events>
    </adminhtml>
</config>
