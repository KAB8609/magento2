<config>
    <modules>
        <Enterprise_Logging>
             <version>0.1.3</version>
        </Enterprise_Logging>
    </modules>
    <global>
        <models>
          <enterprise_logging_mysql4>
            <class>Enterprise_Logging_Model_Mysql4</class>
            <entities>
              <event>
                <table>enterprise_logging_event</table>
              </event>
            </entities>
          </enterprise_logging_mysql4>
          <enterprise_logging>
                <class>Enterprise_Logging_Model</class>
                <resourceModel>enterprise_logging_mysql4</resourceModel>
          </enterprise_logging>
        </models>
        <blocks>
          <enterprise_logging>
            <class>Enterprise_Logging_Block</class>
          </enterprise_logging>
        </blocks>
        <helpers>
          <enterprise_logging>
            <class>Enterprise_Logging_Helper</class>
          </enterprise_logging>
        </helpers>
        <resources>
            <enterprise_logging_setup>
                <setup>
                    <module>Enterprise_Logging</module>
                    <class>Mage_Eav_Model_Entity_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </enterprise_logging_setup>
            <enterprise_logging_write>
              <connection>
                <use>core_write</use>
              </connection>
            </enterprise_logging_write>
            <enterprise_logging_read>
              <connection>
                <use>core_read</use>
              </connection>
            </enterprise_logging_read>
        </resources>

    </global>
    <adminhtml>
        <events>
          <controller_action_predispatch>
            <observers>
              <enterprise_logging>
                <type>singleton</type>
                <class>enterprise_logging/observer</class>
                <method>catchActionStart</method>
              </enterprise_logging>
            </observers>
          </controller_action_predispatch>

          <controller_action_postdispatch_adminhtml>
            <observers>
              <enterprise_logging>
                <type>singleton</type>
                <class>enterprise_logging/observer</class>
                <method>catchActionEnd</method>
              </enterprise_logging>
            </observers>
          </controller_action_postdispatch_adminhtml>

          <model_save_after>
              <observers>
                  <enterprise_logging>
                      <type>singleton</type>
                      <class>enterprise_logging/observer</class>
                      <method>catchModelAfterSave</method>
                  </enterprise_logging>
              </observers>
          </model_save_after>

