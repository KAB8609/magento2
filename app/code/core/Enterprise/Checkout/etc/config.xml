<config>
    <modules>
        <Enterprise_Checkout>
            <version>1.11.0.0</version>
        </Enterprise_Checkout>
    </modules>
    <global>
        <logging_actions>
            <apply_coupon translate="label" module="enterprise_checkout">
                <label>Apply Coupon</label>
            </apply_coupon>
            <add_to_cart translate="label" module="enterprise_checkout">
                <label>Add To Cart</label>
            </add_to_cart>
        </logging_actions>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <enterprise_checkout before="Mage_Adminhtml">Enterprise_Checkout_Adminhtml</enterprise_checkout>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <enterprise_checkout module="Enterprise_Checkout">
                    <file>layout.xml</file>
                </enterprise_checkout>
            </updates>
        </layout>
        <translate>
            <modules>
                <Enterprise_Checkout>
                    <files>
                        <default>Enterprise_Checkout.csv</default>
                    </files>
                </Enterprise_Checkout>
            </modules>
        </translate>
    </adminhtml>
</config>