<logging>
  <actions>
    <crypt_key_save>
      <event>pci</event>
      <action>key_change</action>
      <handler>enterprise_logging/observer::getpcikeychangeaction</handler>
    </crypt_key_save>

    <locks_massUnlock>
      <event>pci</event>
      <action>massUpdate</action>
      <id>unlock</id>
    </locks_massUnlock>
  </actions>
  <labels>
      <list>
          <pci>Pci</pci>
      </list>
  </labels>
</logging>
