<logging>
    <enterprise_cms_hierarchy translate="label">
        <label>CMS Hierarchy</label>
        <expected_models>
            <enterprise_cms__hierarchy_node/>
        </expected_models>
        <actions>
            <adminhtml_cms_hierarchy_index>
                <action>view</action>
                <expected_models />
                <post_dispatch>postDispatchCmsHierachyView</post_dispatch>
            </adminhtml_cms_hierarchy_index>
            <adminhtml_cms_hierarchy_save>
                <action>save</action>
            </adminhtml_cms_hierarchy_save>
        </actions>
    </enterprise_cms_hierarchy>

    <enterprise_cms_page_revision translate="label">
        <label>CMS Revision</label>
        <expected_models>
            <enterprise_cms__page_revision/>
        </expected_models>
        <actions>
            <adminhtml_cms_page_revision_edit>
                <action>view</action>
            </adminhtml_cms_page_revision_edit>

            <adminhtml_cms_page_revision_preview>
                <action>preview</action>
            </adminhtml_cms_page_revision_preview>

            <adminhtml_cms_page_revision_save>
                <action>save</action>
            </adminhtml_cms_page_revision_save>

            <adminhtml_cms_page_revision_delete>
                <action>delete</action>
            </adminhtml_cms_page_revision_delete>
        </actions>
    </enterprise_cms_page_revision>

    <enterprise_cms_page_version translate="label">
        <label>CMS Version</label>
        <expected_models>
            <enterprise_cms__page_version/>
        </expected_models>
        <actions>
            <adminhtml_cms_page_version_edit>
                <action>view</action>
            </adminhtml_cms_page_version_edit>

            <adminhtml_cms_page_version_save>
                <action>save</action>
            </adminhtml_cms_page_version_save>

            <adminhtml_cms_page_version_delete>
                <action>delete</action>
            </adminhtml_cms_page_version_delete>

            <adminhtml_cms_page_version_massDeleteRevisions>
                <action>massDelete</action>
            </adminhtml_cms_page_version_massDeleteRevisions>
        </actions>
    </enterprise_cms_page_version>

    <enterprise_cms_pages>
        <expected_models>
            <enterprise_cms__page_version/>
        </expected_models>
        <actions>
            <adminhtml_cms_page_massDeleteVersions>
                <action>massDelete</action>
            </adminhtml_cms_page_massDeleteVersions>
        </actions>
    </enterprise_cms_pages>
</logging>
