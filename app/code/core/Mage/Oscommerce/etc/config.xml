<config>
    <modules>
        <Mage_Oscommerce>
            <version>0.8.0</version>
            <depends>
                <Mage_Core />
            </depends>
        </Mage_Oscommerce>
    </modules>
    <global>
        <models>
            <oscommerce>
                <class>Mage_Oscommerce_Model</class>
                <resourceModel>oscommerce_mysql4</resourceModel>
            </oscommerce>
            <oscommerce_mysql4>
                <class>Mage_Oscommerce_Model_Mysql4</class>
                <entities>
                    <oscommerce>
                        <table>oscommerce_import</table>
                    </oscommerce>
                </entities>
            </oscommerce_mysql4>
        </models>
        <resources>
            <oscommerce_setup>
                <setup>
                    <module>Mage_Oscommerce</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </oscommerce_setup>
            <oscommerce_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </oscommerce_write>
            <oscommerce_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </oscommerce_read>
        </resources>
    </global>
</config>