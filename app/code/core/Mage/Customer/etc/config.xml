<?xml version="1.0"?>
<config>
    <modules>
        <Mage_Customer>
            <version>0.5.1</version>
            <depends>
                <Mage_Eav />
            </depends>
            <crypt>
                <key><![CDATA[h$$@j>21#Nn 34{}~|][fksn2)*@H NdfM99f+<lml1(23nR_JN]]></key>
            </crypt>
            <wishlistActive>true</wishlistActive>
        </Mage_Customer>
    </modules>
    <global>
        <block>
            <groups><customer><class>Mage_Customer_Block</class></customer></groups>
        </block>
        <models>
            <customer>
                <class>Mage_Customer_Model</class>
                <resourceModel>customer_entity</resourceModel>
            </customer>
            <customer_entity>
                <class>Mage_Customer_Model_Entity</class>
                <entities>
                    <entity>
                        <table>customer_entity</table>
                    </entity>
                    <value_prefix>
                        <table>customer_entity</table>
                    </value_prefix>
                    <customer_group>
                        <table>customer_group</table>
                    </customer_group>
                </entities>
            </customer_entity>
            <customer_mysql4>
                <class>Mage_Customer_Model_Mysql4</class>
                <entities>
                    <customer>
                        <table>customer</table>
                    </customer>
                    <entity>
                        <table>customer_entity</table>
                    </entity>
                    <value_prefix>
                        <table>customer_value</table>
                    </value_prefix>
                    <customer_group>
                        <table>customer_group</table>
                    </customer_group>
                    <address>
                        <table>customer_address</table>
                    </address>
                    <address_type>
                        <table>customer_address_type</table>
                    </address_type>
                    <address_type_language>
                        <table>customer_address_type_language</table>
                    </address_type_language>
                    <address_type_link>
                        <table>customer_address_type_link</table>
                    </address_type_link>
                    <payment>
                        <table>customer_payment</table>
                    </payment>
                    <payment_method>
                        <table>customer_payment_method</table>
                    </payment_method>
                    <wishlist>
                        <table>customer_wishlist</table>
                    </wishlist>
                    <customer_log>
                        <table>customer_log</table>
                    </customer_log>
                </entities>
            </customer_mysql4>
        </models>
        <resources>
            <customer_setup>
                <setup>
                    <module>Mage_Customer</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </customer_setup>
            <customer_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </customer_write>
            <customer_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </customer_read>
        </resources>
    </global>
    <frontend>
        <layouts>
            <default>
                <updates>
                    <customer>
                        <file>customer/initLayout.xml</file>
                    </customer>
                </updates>
            </default>
            <customer_login>
                <updates>
                    <default>
                        <file>customer/login.xml</file>
                    </default>
                </updates>
            </customer_login>
            <customer_register>
                <updates>
                    <default>
                        <file>customer/register.xml</file>
                    </default>
                </updates>
            </customer_register>
            <customer_account>
                <updates>
                    <default>
                        <file>customer/account.xml</file>
                    </default>
                </updates>
            </customer_account>
            <customer_dashboard>
                <updates>
                    <default>
                        <file>customer/dashboard.xml</file>
                    </default>
                </updates>
            </customer_dashboard>
            <customer_address>
                <updates>
                    <default>
                        <file>customer/address.xml</file>
                    </default>
                </updates>
            </customer_address>
            <wishlist>
                <updates>
                    <default>
                        <file>customer/wishlist.xml</file>
                    </default>
                </updates>
            </wishlist>
        </layouts>
        <events>
            <beforeGenerateLayoutBlocks>
                <observers>
                    <accountIsLoggedIn>
                        <type>singleton</type>
                        <class>customer/observer</class>
                        <method>beforeGenerateLayoutBlocks</method>
                    </accountIsLoggedIn>
                </observers>
            </beforeGenerateLayoutBlocks>
        </events>
        <routers>
            <customer>
                <use>standard</use>
                <args>
                    <module>Mage_Customer</module>
                    <frontName>customer</frontName>
                </args>
            </customer>
        </routers>
    </frontend>
</config>