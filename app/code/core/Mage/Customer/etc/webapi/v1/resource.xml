<wsdl:definitions name="Magento"
                  targetNamespace="urn:Magento"
                  xmlns:tns="urn:Magento"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
    <wsdl:types>
        <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:Magento">
            <xs:complexType name="customerEntityCreate">
                <xs:sequence>
                    <xs:element name="email" type="xs:string" minOccurs="1" />
                    <xs:element name="firstname" type="xs:string" minOccurs="1" />
                    <xs:element name="lastname" type="xs:string" minOccurs="1" />
                    <xs:element name="password" type="xs:string" minOccurs="1" />
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="customerEntity">
                <xs:sequence>
                    <xs:element name="customer_id" type="xs:int" minOccurs="0" />
                    <xs:element name="created_at" type="xs:string" minOccurs="0" />
                    <xs:element name="firstname" type="xs:string" minOccurs="0" />
                    <xs:element name="middlename" type="xs:string" minOccurs="0" />
                    <xs:element name="lastname" type="xs:string" minOccurs="0" />
                    <xs:element name="password_hash" type="xs:string" minOccurs="0" />
                </xs:sequence>
            </xs:complexType>

            <xs:element name="customerCreateRequestParam">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="1" name="data" type="tns:customerEntityCreate" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="customerCreateResponseParam">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="1" name="result" type="xs:int" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="customerInfoRequestParam">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="1" name="id" type="xs:int" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="customerInfoResponseParam">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="1" name="result" type="tns:customerEntity" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="customerListRequestParam">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="1" name="id" type="xs:int" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="customerListResponseParam">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="1" name="result" type="tns:customerEntity" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="customerUpdateRequestParam">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="1" name="id" type="xs:int" />
                        <xs:element minOccurs="1" maxOccurs="1" name="data" type="tns:customerEntity" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="customerUpdateResponseParam">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="1" name="result" type="xs:int" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="customerDeleteRequestParam">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="1" name="result" type="xs:int" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="customerDeleteResponseParam">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="1" maxOccurs="1" name="result" type="tns:customerEntity" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:schema>
    </wsdl:types>

    <wsdl:message name="customerCreateRequest">
        <wsdl:part name="parameters" element="tns:customerCreateRequestParam" />
    </wsdl:message>
    <wsdl:message name="customerCreateResponse">
        <wsdl:part name="parameters" element="tns:customerCreateResponseParam" />
    </wsdl:message>
    <wsdl:message name="customerInfoRequest">
        <wsdl:part name="parameters" element="tns:customerInfoRequestParam" />
    </wsdl:message>
    <wsdl:message name="customerInfoResponse">
        <wsdl:part name="parameters" element="tns:customerInfoResponseParam" />
    </wsdl:message>
    <wsdl:message name="customerListRequest">
        <wsdl:part name="parameters" element="tns:customerListRequestParam" />
    </wsdl:message>
    <wsdl:message name="customerListResponse">
