    </wsdl:message>
    <wsdl:message name="customerDeleteResponse">
        <wsdl:part name="parameters" element="tns:customerDeleteResponseParam" />
    </wsdl:message>

    <wsdl:portType name="customer">
        <wsdl:operation name="customerCreate">
            <wsdl:documentation>Create customer</wsdl:documentation>
            <wsdl:input message="tns:customerCreateRequest" />
            <wsdl:output message="tns:customerCreateResponse" />
        </wsdl:operation>
        <wsdl:operation name="customerGet">
            <wsdl:documentation>Retrieve customer data</wsdl:documentation>
            <wsdl:input message="tns:customerInfoRequest" />
            <wsdl:output message="tns:customerInfoResponse" />
        </wsdl:operation>
        <wsdl:operation name="customerList">
            <wsdl:documentation>Retrieve customer collection</wsdl:documentation>
