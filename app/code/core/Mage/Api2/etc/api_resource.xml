<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="Magento"
                  targetNamespace="urn:Magento"
                  xmlns:tns="urn:Magento"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
    <wsdl:types>
        <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:Magento">
            <xs:element name="Authorization" type="xs:string"/>
        </xs:schema>
    </wsdl:types>

    <wsdl:message name="AuthorizationHeader">
        <wsdl:part name="Authorization" element="tns:Authorization" />
    </wsdl:message>

</wsdl:definitions>
