<config>
    <modules>
        <Mage_Pickup>
            <version>0.1.1</version>
        </Mage_Pickup>
    </modules>
    <global>
        <models>
            <pickup>
                <class>Mage_Pickup_Model</class>
            </pickup>
        </models>
        <resources>
            <pickup_setup>
                <setup>
                    <module>Mage_Pickup</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </pickup_setup>
        </resources>
    </global>
</config>
