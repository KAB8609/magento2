<config>
    <modules>
        <Mage_Connect>
            <version>1.6.0.0</version>
        </Mage_Connect>
    </modules>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <connect before="Mage_Adminhtml">Mage_Connect_Adminhtml</connect>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <connect module="Mage_Connect">
                    <file>layout.xml</file>
                </connect>
            </updates>
        </layout>
    </adminhtml>
</config>
