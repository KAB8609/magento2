<config>
    <modules>
        <Mage_Moneybookers>
            <version>1.1.5</version>
        </Mage_Moneybookers>
    </modules>
    <global>
        <models>
            <moneybookers>
                <class>Mage_Moneybookers_Model</class>
            </moneybookers>
        </models>
        <helpers>
            <moneybookers>
                <class>Mage_Moneybookers_Helper</class>
            </moneybookers>
        </helpers>
        <resources>
            <moneybookers_setup>
                <setup>
                    <module>Mage_Moneybookers</module>
                </setup>
            </moneybookers_setup>
        </resources>
        <blocks>
            <moneybookers><class>Mage_Moneybookers_Block</class></moneybookers>
        </blocks>
    </global>
    <frontend>
        <routers>
            <moneybookers>
                <use>standard</use>
                <args>
                    <module>Mage_Moneybookers</module>
                    <frontName>moneybookers</frontName>
                </args>
            </moneybookers>
        </routers>
        <translate>
            <modules>
                <Mage_Moneybookers>
                    <files>
                        <default>Mage_Moneybookers.csv</default>
                    </files>
                </Mage_Moneybookers>
            </modules>
        </translate>
        <layout>
            <updates>
                <moneybookers>
                    <file>moneybookers.xml</file>
                </moneybookers>
            </updates>
        </layout>
    </frontend>
    <admin>
         <routers>
            <adminhtml>
                <args>
                    <modules>
                        <moneybookers before="Mage_Adminhtml">Mage_Moneybookers_Adminhtml</moneybookers>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <translate>
            <modules>
                <Mage_Moneybookers>
                    <files>
                        <default>Mage_Moneybookers.csv</default>
                    </files>
                </Mage_Moneybookers>
            </modules>
        </translate>
        <layout>
            <updates>
                <moneybookers>
                    <file>moneybookers.xml</file>
                </moneybookers>
            </updates>
        </layout>
        <acl>
            <resources>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <moneybookers translate="title" module="moneybookers">
                                            <title>Moneybookers Settings</title>
                                        </moneybookers>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>
    <phoenix>
        <moneybookers>
            <verify_url>http://www.phoenix-medien.de/index.php?id=105</verify_url>
        </moneybookers>
    </phoenix>
    <default>
        <payment>
            <moneybookers_acc translate="title" module="moneybookers">
                <active>0</active>
                <model>moneybookers/acc</model>
                <order_status>1</order_status>
                <title>Credit Card / Visa, Mastercard, AMEX, JCB, Diners</title>
