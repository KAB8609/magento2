<?xml version="1.0"?>
<config>
   <modules>
      <Mage_Adminhtml>
         <version>0.2.0</version>
         <depends>
            <Mage_Core />
         </depends>
      </Mage_Adminhtml>
   </modules>

   <global>
        <models>
            <adminhtml><class>Mage_Adminhtml_Model</class></adminhtml>
        </models>
        <block>
            <groups><adminhtml><class>Mage_Adminhtml_Block</class></adminhtml></groups>
        </block>
   </global>
   <front>
      <layouts>
         <adminhtml>
            <updates>
               <adminhtml>
                  <file>adminhtml/init.xml</file>
               </adminhtml>
            </updates>
         </adminhtml>
         <baseframe>
            <updates>
               <baseframe>
                  <file>adminhtml/baseframe.xml</file>
               </baseframe>
            </updates>
         </baseframe>
      </layouts>
      <events>
         <action_preDispatch_Mage_Adminhtml>
            <observers>
               <auth>
                  <type>singleton</type>
                  <class>admin/observer</class>
                  <method>actionPreDispatchAdmin</method>
               </auth>
            </observers>
         </action_preDispatch_Mage_Adminhtml>
      </events>
      <routers>
         <adminhtml>
            <class>Mage_Core_Controller_Front_Router</class>
            <args>
               <module>Mage_Adminhtml</module>
               <frontName>adminhtml</frontName>
            </args>
         </adminhtml>
      </routers>
   </front>
</config>
