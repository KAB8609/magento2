<?xml version="1.0"?>
<config>
    <admin>
        <menu>
             <sales>
                <title>Sales</title>
                <!-- action>adminhtml/sales</action -->
                <depends><module>Mage_Sales</module></depends>
                <children>
                    <order>
                        <title>Orders</title>
                        <action>adminhtml/sales_order</action>
                    </order>
                    <invoice>
                        <title>Invoices</title>
                        <action>adminhtml/sales_invoice</action>
                    </invoice>
                    <tax>
                        <title>Tax</title>
                        <children>
                            <rules>
                                <title>Manage Tax Rules</title>
                                <action>adminhtml/tax_rule</action>
                            </rules>
                            <rates>
                                <title>Manage Tax Rates</title>
                                <action>adminhtml/tax_rate</action>
                            </rates>
                            <import_export>
                                <title>Import / Export Tax Rates</title>
                                <action>adminhtml/tax_rate/importExport</action>
                            </import_export>
                            <classes_customer>
                                <title>Customer Tax Classes</title>
                                <action>adminhtml/tax_class_customer</action>
                            </classes_customer>
                            <classes_product>
                                <title>Product Tax Classes</title>
                                <action>adminhtml/tax_class_product</action>
                            </classes_product>
                        </children>
                    </tax>
                    <config>
                        <title>Configuration</title>
                        <action>adminhtml/sales_config/</action>
                    </config>
                </children>
             </sales>
             <catalog>
                <title>Catalog</title>
                <!--action>adminhtml/catalog</action-->
                <depends><module>Mage_Catalog</module></depends>
                <children>
                    <products>
                        <title>Manage Products</title>
                        <action>adminhtml/catalog_product/</action>
                    </products>
                    <categories>
                        <title>Manage Categories</title>
                        <action>adminhtml/catalog_category/</action>
                    </categories>
                    <attributes>
                        <title>Attributes</title>
                        <children>
                            <attributes>
                                <title>Manage Attributes</title>
                                <action>adminhtml/catalog_product_attribute/</action>
                            </attributes>
                            <sets>
                                <title>Manage Attribute Sets</title>
                                <action>adminhtml/catalog_product_set/</action>
                            </sets>
                        </children>
                    </attributes>
                    <reviews_ratings>
                        <title>Reviews &amp; Ratings</title>
                        <children>
                            <reviews>
                                <title>Customer Reviews</title>
                                <action>adminhtml/catalog_product_review/</action>
                            </reviews>
                            <ratings>
                                <title>Manage Ratings</title>
                                <action>adminhtml/rating/</action>
                            </ratings>
                        </children>
                    </reviews_ratings>
                    <tag>
                        <title>Tags</title>
                        <children>
                            <pending>
                                <title>Pending Tags</title>
                                <action>adminhtml/tag/pending</action>
                            </pending>
                            <all>
                                <title>All Tags</title>
                                <action>adminhtml/tag</action>
                            </all>
                            <!--
                            <customer>
                                <title>Customers' Tags</title>
                                <action>adminhtml/tag/customer</action>
                            </customer>
                            <product>
                                <title>Products' Tags</title>
                                <action>adminhtml/tag/product</action>
                            </product>
                            -->
                        </children>
                    </tag>
                    <search>
                        <title>Search</title>
                        <action>adminhtml/catalog_search/</action>
                    </search>
                    <!--<datafeed>
                        <title>Datafeeds</title>
                        <action>adminhtml/catalog_datafeed/</action>
                    </datafeed>-->
                    <config>
                        <title>Configuration</title>
                        <action>adminhtml/system_config/edit/section/catalog/</action>
                    </config>
                </children>
             </catalog>
            <!--
             <dashboard>
                <title>Dashboard</title>
                <action>adminhtml/dashboard/</action>
                <acl><resource>admin/dashboard</resource><privilege>view</privilege></acl>
             </dashboard>
             -->
             <customer>
                <title>Customers</title>
                <!-- action>adminhtml/customer/</action -->
                <depends><module>Mage_Customer</module></depends>
                <children>
                    <manage>
                        <title>Manage Customers</title>
                        <action>adminhtml/customer/</action>
                        <acl><resource>admin/customer</resource><privilege>list</privilege></acl>
                    </manage>
                    <group>
                        <title>Customer Groups</title>
                        <action>adminhtml/customer_group/</action>
                        <resource>admin/customer/group</resource>
                    </group>
                    <online>
                        <title>Online Customers</title>
                        <action>adminhtml/customer_online/</action>
                        <resource>admin/customer/online</resource>
                    </online>
                    <config>
                        <title>Configuration</title>
                        <action>adminhtml/system_config/edit/section/customer/</action>
                        <resource>admin/customer/config</resource>
                    </config>
                </children>
             </customer>
             <promo>
                <title>Promotions</title>
                <!-- action>adminhtml/promo</action -->
                <children>
                    <catalog>
                        <title>Catalog Price Rules</title>
                        <action>adminhtml/promo_catalog/</action>
                        <resource>promo/catalog</resource>
                        <depends><module>Mage_Catalog</module></depends>
                    </catalog>
                    <quote>
                        <title>Shopping Cart Price Rules</title>
                        <action>adminhtml/promo_quote/</action>
                        <resource>promo/quote</resource>
                        <depends><module>Mage_Sales</module></depends>
                    </quote>
                    <!--<config>
                        <title>Configuration</title>
                        <action>adminhtml/system_config/</action>
                        <resource>promo/config</resource>
                    </config>-->
                </children>
             </promo>
             <cms>
                <title>CMS</title>
                <children>
                    <page>
                        <title>Manage Pages</title>
                        <action>adminhtml/cms_page</action>
                    </page>
                    <block>
                        <title>Static Blocks</title>
                        <action>adminhtml/cms_block</action>
                    </block>
                    <poll>
                        <title>Poll Manager</title>
                        <action>adminhtml/poll</action>
                    </poll>
                    <!--<config>
                        <title>Configuration</title>
                        <action>adminhtml/cms_config/</action>
                        <resource>cms/config</resource>
                    </config>-->
                </children>
             </cms>
             <newsletter>
                <title>Newsletter</title>
                <!-- action>adminhtml/newsletter/</action -->
                <!--<acl><resource>admin/newsletter</resource><privilege>list</privilege></acl>-->
                <depends><module>Mage_Newsletter</module></depends>
                <children>
                    <template>
                        <title>Newsletter Templates</title>
                        <action>adminhtml/newsletter_template/</action>
                        <resource>admin/newsletter/template</resource>
                    </template>
                    <!-- <subscriber>
                        <title>Subscribers</title>
                        <action>adminhtml/newsletter_subscriber/</action>
                        <resource>admin/newsletter/subscriber</resource>
                    </subscriber> -->
                    <queue>
                        <title>Newsletter Queue</title>
                        <action>adminhtml/newsletter_queue/</action>
                        <resource>admin/newsletter/queue</resource>
                    </queue>
                    <problem>
                        <title>Newsletter Problem Reports</title>
                        <action>adminhtml/newsletter_problem/</action>
                        <resource>admin/newsletter/problem</resource>
                    </problem>
                    <config>
                        <title>Configuration</title>
                        <action>adminhtml/system_config/edit/section/newsletter/</action>
                        <resource>admin/newsletter/config</resource>
                    </config>
                </children>
             </newsletter>
             <report>
                <title>Reports</title>
                <children>
                    <sales>
                        <title>Sales Report</title>
                        <action>adminhtml/report/sales</action>
                    </sales>
                    <shopcart>
                        <title>Shopping Cart Report</title>
                        <action>adminhtml/report/shopcart</action>
                    </shopcart>
                    <products>
                        <title>Products Report</title>
                        <action>adminhtml/report/products</action>
                    </products>
                    <coupons>
                        <title>Coupons Reports</title>
                        <action>adminhtml/report/coupons</action>
                    </coupons>
                    <wishlist>
                        <title>Wishlist Report</title>
                        <action>adminhtml/report/wishlist</action>
                    </wishlist>
                    <review>
                        <title>Reviews Report</title>
                        <children>
                            <customer>
                                <title>Customers Reviews</title>
                                <action>adminhtml/report_review/customer</action>
                            </customer>
                            <product>
                                <title>Products Reviews</title>
                                <action>adminhtml/report_review/product</action>
                            </product>
                        </children>
                    </review>
                    <tags>
                        <title>Tags Report</title>
                        <children>
                            <customer>
                                <title>Customers</title>
                                <action>adminhtml/report_tag/customer</action>
                            </customer>
                            <product>
                                <title>Products</title>
                                <action>adminhtml/report_tag/product</action>
                            </product>
                            <product_total>
                                <title>Products (Total)</title>
                                <action>adminhtml/report_tag/productAll</action>
                            </product_total>
                            <popular>
                                <title>Popular</title>
                                <action>adminhtml/report_tag/popular</action>
                            </popular>
                        </children>
                    </tags>
                    <search>
                        <title>Search Report</title>
                        <action>adminhtml/report/search</action>
                    </search>
                    <customers>
                        <title>Best Customers</title>
                        <action>adminhtml/report/customers</action>
                    </customers>
                    <orders>
                        <title>Recent Orders</title>
                        <action>adminhtml/report/orders</action>
                    </orders>
                    <totals>
                        <title>Order Totals</title>
                        <action>adminhtml/report/totals</action>
                    </totals>
                </children>
             </report>
             <system>
                <title>System</title>
                <!-- action>adminhtml/system</action -->
                <children>
                    <myaccount>
                        <title>My Account</title>
                        <action>adminhtml/system_account</action>
                    </myaccount>
                    <tools>
                        <title>Tools</title>
                        <children>
                            <backup>
                                <title>Backups</title>
                                <action>adminhtml/system_backup</action>
                            </backup>
                        </children>
                    </tools>
                    <currency>
                        <title>Manage Currency</title>
                        <action>adminhtml/system_currency</action>
                    </currency>
                    <email_template>
                        <title>Transactional Emails</title>
                        <action>adminhtml/system_email_template</action>
                    </email_template>
                    <acl>
                        <title>Permissions</title>
                        <children>
                            <users>
                                <title>Users</title>
                                <action>adminhtml/permission_user</action>
                            </users>
                            <roles>
                                <title>Roles</title>
                                <action>adminhtml/permission_role</action>
                            </roles>
                        </children>
                    </acl>
                    <config>
                        <title>Configuration</title>
                        <action>adminhtml/system_config</action>
                    </config>
                </children>
             </system>
        </menu>

    </admin>
</config>