<?xml version="1.0"?>
<config>
    <admin>
      <menu>
         <dashboard>
            <title>Dashboard</title>
            <action>adminhtml/dashboard/</action>
            <!--<acl><resource>admin/dashboard</resource><privilege>view</privilege></acl>-->
         </dashboard>
         <customer>
            <title>Customers</title>
            <action>adminhtml/customer/</action>
            <acl><resource>admin/customer</resource><privilege>list</privilege></acl>
            <depends><module>Mage_Customer</module></depends>
            <children>
                <group>
                    <title>Customer groups</title>
                    <action>adminhtml/customer_group/</action>
                    <resource>admin/customer/group</resource>
                </group>
                <online>
                    <title>View online customers</title>
                    <action>adminhtml/customer/online/</action>
                    <resource>admin/customer/online</resource>
                </online>
                <config>
                    <title>Configuration</title>
                    <action>adminhtml/customer_config/</action>
                    <resource>admin/customer/config</resource>
                </config>
            </children>
         </customer>
         <catalog>
            <title>Catalog</title>
            <action>adminhtml/catalog</action>
            <depends><module>Mage_Catalog</module></depends>
            <children>
                <attributes>
                    <title>Product attributes</title>
                    <action>adminhtml/productAttributes/</action>
                </attributes>
                <reviews>
                    <title>Reviews</title>
                    <action>adminhtml/reviews/</action>
                </reviews>
                <tags>
                    <title>Tags</title>
                    <action>adminhtml/tag/</action>
                </tags>
                <datafeed>
                    <title>Datafeeds</title>
                    <action>adminhtml/datafeeds/</action>
                </datafeed>
                <config>
                    <title>Configuration</title>
                    <action>adminhtml/catalogConfig/</action>
                </config>
            </children>
         </catalog>
         <sales>
            <title>Sales</title>
            <action>adminhtml/sales</action>
            <depends><module>Mage_Sales</module></depends>
            <children>
                <config>
                    <title>Configuration</title>
                    <action>adminhtml/salesConfig/</action>
                </config>
            </children>
         </sales>
         <promo>
            <title>Promotions</title>
            <action>adminhtml/promo</action>
            <children>
                <catalog>
                    <title>Catalog price rules</title>
                    <action>adminhtml/catalogPriceRules/</action>
                    <resource>promo/catalog</resource>
                    <depends><module>Mage_Catalog</module></depends>
                </catalog>
                <quote>
                    <title>Shopping cart price rules</title>
                    <action>adminhtml/quotePriceRules/</action>
                    <resource>promo/quote</resource>
                    <depends><module>Mage_Sales</module></depends>
                </quote>
                <config>
                    <title>Configuration</title>
                    <action>adminhtml/promoConfig/</action>
                    <resource>promo/config</resource>
                </config>
            </children>
         </promo>
         <cms>
            <title>CMS</title>
            <action>adminhtml/cms</action>
            <children>
                <config>
                    <title>Configuration</title>
                    <action>adminhtml/cmsConfig/</action>
                    <resource>cms/config</resource>
                </config>
            </children>
         </cms>
         <report>
            <title>Reports</title>
            <action>adminhtml/report</action>
            <children/>
         </report>
         <system>
            <title>System</title>
            <action>adminhtml/system</action>
            <children>
                <myaccount>
                    <title>My Account</title>
                    <action>adminhtml/myAccount</action>
                </myaccount>
                <tools>
                    <title>Tools</title>
                    <children>
                        <backup>
                            <title>Backup</title>
                            <action>adminhtml/backup</action>
                        </backup>
                    </children>
                </tools>
                <config>
                    <title>Configuration</title>
                    <action>adminhtml/system_config</action>
                    <children>
                        <modules>
                            <title>Modules</title>
                            <action>adminhtml/moduleConfig</action>
                        </modules>
                        <acl>
                            <title>Permissions</title>
                            <action>adminhtml/acl</action>
                        </acl>
                        <resources>
                            <title>Resources</title>
                            <action>adminhtml/resources</action>
                        </resources>
                    </children>
                </config>
            </children>
         </system>
      </menu>
    </admin>
</config>
