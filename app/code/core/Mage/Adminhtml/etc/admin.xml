<?xml version="1.0"?>
<config>
    <admin>
      <acl>
         <asserts>
            <ip><class>Mage_Admin_Model_Acl_Assert_Ip</class></ip>
            <time><class>Mage_Admin_Model_Acl_Assert_Time</class></time>
         </asserts>

         <resources>
            <admin>
                <system>
                   <permissions />
                   <modules />
                   <websites />
                </system>
                <catalog>
                   <product />
                   <product_attributes />
                   <product_datafeeds />
                </catalog>
                <customer>
                   <address />
                </customer>
                <paygate>
                   <authorizenet />
                   <paypal />
                   <verisign />
                </paygate>
                <sales>
                   <orders />
                </sales>
            </admin>
         </resources>

         <privilegeSets>
            <default>
               <view descr="View entity"/>
               <edit descr="Edit entity"/>
               <delete descr=""/>
               <create descr=""/>
            </default>
         </privilegeSets>
      </acl>

      <search>
         <global>
            <collections>
               <products>
                  <class>catalog/admin_search</class>
               </products>
               <customers>
                  <class>customers/admin_search</class>
               </customers>
               <sales>
                  <class>sales/admin_search</class>
               </sales>
            </collections>
         </global>
      </search>
      
      <nav>
         <dashboard>
            <title>Dashboard</title>
            <action>adminhtml/index/dashboard</action>
            <resource>dashboard</resource>
         </dashboard>
         <customer>
            <title>Customers</title>
            <action>adminhtml/customer</action>
            <resource>customer</resource>
            <children>
                <manage>
                    <title>Manage customers</title>
                    <action>adminhtml/customer/list</action>
                    <resource>customer/list</resource>
                </manage>
         </customer>
         <catalog>
            <title>Catalog</title>
            <children>
                <products>
                    <title>Categories and Products</title>
                </products>
            <attributes>
                <title>Product attributes</title>
            </attributes>
            </children>
        </catalog>
         <sales>
            <title>Sales</title>
            <children />
         </sales>
         <system>
            <title>System</title>
            <children />
         </system>
      </nav>
    </admin>
    <translate></translate>
</config>