<?xml version="1.0"?>
<config>
    <admin>
      <acl>
         <asserts>
            <ip><class>Mage_Admin_Model_Acl_Assert_Ip</class></ip>
            <time><class>Mage_Admin_Model_Acl_Assert_Time</class></time>
         </asserts>

         <resources>
            <system>
               <permissions />
               <modules />
               <websites />
            </system>
            <catalog>
               <categories_products />
               <categories_attributes />
               <products_attributes />
               <products_datafeeds />
            </catalog>
            <customers>
               <manage />
            </customers>
            <paygate>
               <authorizenet />
               <paypal />
               <verisign />
            </paygate>
            <sales>
               <orders />
            </sales>
         </resources>

         <privilegeSets>
            <default>
               <view />
               <edit />
               <delete />
               <create />
            </default>
         </privilegeSets>
      </acl>

      <search>
         <global>
            <collections>
               <products>
                  <class>Mage_Catalog_Model_Admin_Search</class>
               </products>
               <customers>
                  <class>Mage_Customer_Model_Admin_Search</class>
               </customers>
               <sales>
                  <class>Mage_Sales_Model_Admin_Search</class>
               </sales>
            </collections>
         </global>
      </search>
    </admin>
    <translate></translate>
</config>