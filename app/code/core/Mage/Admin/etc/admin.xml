<?xml version="1.0"?>
<config>
    <global>
        <block>
            <types>
                <admin_catalog_category_form>
                    <class>Mage_Catalog_Block_Admin_Category_Form</class>
                </admin_catalog_category_form>
                <admin_catalog_category_form_json>
                    <class>Mage_Catalog_Block_Admin_Category_FormJson</class>
                </admin_catalog_category_form_json>
                <admin_catalog_product_create_option>
                    <class>Mage_Catalog_Block_Admin_Product_CreateOption</class>
                </admin_catalog_product_create_option>
                <admin_catalog_product_form>
                    <class>Mage_Catalog_Block_Admin_Product_Form</class>
                </admin_catalog_product_form>
                <admin_catalog_product_form_json>
                    <class>Mage_Catalog_Block_Admin_Product_FormJson</class>
                </admin_catalog_product_form_json>
                <admin_catalog_product_card>
                    <class>Mage_Catalog_Block_Admin_Product_Card</class>
                </admin_catalog_product_card>
            </types>
        </block>
    </global>
    <admin>
      <acl>
         <asserts>
            <ip><class>Mage_Admin_Model_Acl_Assert_Ip</class></ip>
            <time><class>Mage_Admin_Model_Acl_Assert_Time</class></time>
         </asserts>

         <resources>
            <system>
               <permissions />
               <modules />
               <websites />
            </system>
            <catalog>
               <categories_products />
               <categories_attributes />
               <products_attributes />
               <products_datafeeds />
            </catalog>
            <customers>
               <manage />
            </customers>
            <paygate>
               <authorizenet />
               <paypal />
               <verisign />
            </paygate>
            <sales>
               <orders />
            </sales>
         </resources>

         <privilegeSets>
            <default>
               <view />
               <edit />
               <delete />
               <create />
            </default>
         </privilegeSets>
      </acl>

      <layouts>
         <default>
            <updates>
               <core>
                  <file>core/initLayout.xml</file>
               </core>
               <auth>
                  <file>auth/initLayout.xml</file>
               </auth>
               <catalog>
                  <file>catalog/initLayout.xml</file>
               </catalog>
               <customer>
                  <file>customer/initLayout.xml</file>
               </customer>
               <sales>
                  <file>sales/initLayout.xml</file>
               </sales>
            </updates>
         </default>
      </layouts>

      <events>
         <action_preDispatch>
            <observers>
               <auth>
                  <class>Mage_Admin_Model_Auth</class>
                  <method>action_preDispatch</method>
               </auth>
            </observers>
         </action_preDispatch>
      </events>

      <search>
         <global>
            <collections>
               <products>
                  <class>Mage_Catalog_Model_Admin_Search</class>
               </products>
               <customers>
                  <class>Mage_Customer_Model_Admin_Search</class>
               </customers>
               <sales>
                  <class>Mage_Sales_Model_Admin_Search</class>
               </sales>
            </collections>
         </global>
      </search>

      <translate> 
      
      </translate>
    </admin>
</config>