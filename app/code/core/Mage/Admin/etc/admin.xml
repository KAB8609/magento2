<?xml version="1.0"?>
<config>
    <admin>
      <acl>
         <asserts>
            <ip><class>Mage_Admin_Model_Acl_Assert_Ip</class></ip>
            <time><class>Mage_Admin_Model_Acl_Assert_Time</class></time>
         </asserts>

         <resources>
            <admin>
				<catalog>
					<attributes>
						<attributes/>
						<sets/>
					</attributes>
					<categories/>
					<config/>
					<products/>
					<reviews_ratings>
						<ratings/>
						<reviews>
							<all/>
							<pending/>
						</reviews>
					</reviews_ratings>
					<search/>
					<tag>
						<all/>
						<pending/>
					</tag>
				</catalog>
				<cms>
					<block/>
					<page/>
					<poll/>
				</cms>
				<customer>
					<config/>
					<group/>
					<manage/>
					<online/>
				</customer>
				<newsletter>
					<config/>
					<problem/>
					<queue/>
					<template/>
				</newsletter>
				<promo>
					<catalog/>
					<quote/>
				</promo>
				<report>
					<coupons/>
					<customers/>
					<orders/>
					<products/>
					<review>
						<customer/>
						<product/>
					</review>
					<sales/>
					<search/>
					<shopcart/>
					<tags>
						<customer/>
						<popular/>
						<product/>
						<product_total/>
					</tags>
					<totals/>
					<wishlist/>
				</report>
				<sales>
					<config/>
					<invoice/>
					<order/>
					<tax>
						<classes_customer/>
						<classes_product/>
						<import_export/>
						<rates/>
						<rules/>
					</tax>
				</sales>
				<system>
					<acl>
						<roles/>
						<users/>
					</acl>
					<config/>
					<currency/>
					<email_template/>
					<myaccount/>
					<tools>
						<backup/>
					</tools>
				</system>
            </admin>
         </resources>

         <privilegeSets>
            <default>
               <view descr="View entity"/>
               <edit descr="Edit entity"/>
               <delete descr=""/>
               <create descr=""/>
            </default>
         </privilegeSets>
      </acl>

      <search>
         <global>
            <collections>
               <products>
                  <class>catalog/admin_search</class>
               </products>
               <customers>
                  <class>customer/admin_search</class>
               </customers>
               <sales>
                  <classa>sales/admin_search</classa>
               </sales>
            </collections>
         </global>
      </search>
    </admin>
    <translate></translate>
</config>