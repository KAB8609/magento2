<?xml version="1.0"?>
<config>
    <modules>
        <Mage_Core>
             <version>0.2.2</version>
        </Mage_Core>
    </modules>

    <global>
        <resource>
            <connection>
                <types>
                    <pdo_mysql>
                        <class>Mage_Core_Model_Resource_Type_Db_Pdo_Mysql</class>
                    </pdo_mysql>
                    <mysqli>
                        <class>Mage_Core_Model_Resource_Type_Db_Mysqli</class>
                    </mysqli>
                    <mysqli_setup>
                        <class>Mage_Core_Model_Resource_Type_Db_Mysqli_Setup</class>
                    </mysqli_setup>
                </types>
            </connection>
        </resource>

        <resources>
            <core_setup>
                <setup>
                    <module>Mage_Core</module>
                </setup>
            </core_setup>
        </resources>

        <models>
            <core>
                <class>Mage_Core_Model</class>
                <resourceModel>core_mysql4</resourceModel>
            </core>

            <core_mysql4>
                <class>Mage_Core_Model_Mysql4</class>
                <entities>
                    <config>
                        <table>core_config</table>
                    </config>
                    <website>
                        <table>core_website</table>
                    </website>
                    <language>
                        <table>core_language</table>
                    </language>
                    <resource>
                        <table>core_resource</table>
                    </resource>
                    <module>
                        <table>core_module</table>
                    </module>
                    <table>
                        <table>core_table</table>
                    </table>
                    <setting>
                        <table>core_setting</table>
                    </setting>
                    <session>
                        <table>core_session</table>
                    </session>
                    <data_change>
                        <table>core_data_change</table>
                    </data_change>
                    <data_change_type>
                        <table>core_data_change_type</table>
                    </data_change_type>
                    <data_change_info>
                        <table>core_data_change_info</table>
                    </data_change_info>
                </entities>
            </core_mysql4>
        </models>

        <block>
            <groups><core><class>Mage_Core_Block</class></core></groups>
        </block>
    </global>
    <front>
        <layouts>
            <noRoute>
                <updates>
                    <core>
                        <file>core/noRoute.xml</file>
                    </core>
                </updates>
            </noRoute>
        <default>
            <updates>
                <page>
                    <file>core/default.xml</file>
                </page>
            </updates>
        </default>
        </layouts>
    </front>
    <translate></translate>
</config>