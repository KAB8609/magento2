<?xml version="1.0"?>
<config>
    <modules>
        <Mage_Core>
             <version>0.3.3</version>
        </Mage_Core>
    </modules>

    <global>
        <resource>
            <connection>
                <types>
                    <pdo_mysql>
                        <class>Mage_Core_Model_Resource_Type_Db_Pdo_Mysql</class>
                    </pdo_mysql>
                    <mysqli>
                        <class>Mage_Core_Model_Resource_Type_Db_Mysqli</class>
                    </mysqli>
                    <mysqli_setup>
                        <class>Mage_Core_Model_Resource_Type_Db_Mysqli_Setup</class>
                    </mysqli_setup>
                </types>
            </connection>
        </resource>

        <resources>
            <core_setup>
                <setup>
                    <module>Mage_Core</module>
                </setup>
            </core_setup>
        </resources>

        <entity>
            <types>
                <default>
                    <resource><table>core/entity</table></resource>
                    <attribute>
                        <resource><table>core/entity_attribute</table></resource>
                        <types>
                            <varchar>
                                <resource><table>core/entity_attribute_varchar</table></resource>
                            </varchar>
                            <decimal>
                                <resource><table>core/entity_attribute_decimal</table></resource>
                            </decimal>
                            <datetime>
                                <resource><table>core/entity_attribute_datetime</table></resource>
                            </datetime>
                            <text>
                                <resource><table>core/entity_attribute_text</table></resource>
                            </text>
                            <int>
                                <resource><table>core/entity_attribute_int</table></resource>
                            </int>
                        </types>
                    </attribute>
                </default>
            </types>
        </entity>

        <models>
            <core>
                <class>Mage_Core_Model</class>
                <resourceModel>core_mysql4</resourceModel>
            </core>

            <core_mysql4>
                <class>Mage_Core_Model_Mysql4</class>
                <entities>
                    <resource>
                        <table>core_resource</table>
                    </resource>
                    <language>
                        <table>core_language</table>
                    </language>
                    <website>
                        <table>core_website</table>
                    </website>
                    <store>
                        <table>core_store</table>
                    </store>
                    <session>
                        <table>core_session</table>
                    </session>
                    <entity_type>
                        <table>core_entity_type</table>
                    </entity_type>
                    <entity>
                        <table>core_entity</table>
                    </entity>
                    <entity_attribute>
                        <table>core_attribute</table>
                    </entity_attribute>
                    <entity_attribute_int>
                        <table>core_attribute_int</table>
                    </entity_attribute_int>
                    <entity_attribute_datetime>
                        <table>core_attribute_datetime</table>
                    </entity_attribute_datetime>
                    <entity_attribute_text>
                        <table>core_attribute_text</table>
                    </entity_attribute_text>
                    <entity_attribute_decimal>
                        <table>core_attribute_decimal</table>
                    </entity_attribute_decimal>
                    <entity_attribute_varchar>
                        <table>core_attribute_varchar</table>
                    </entity_attribute_varchar>
                </entities>
            </core_mysql4>
        </models>

        <block>
            <groups><core><class>Mage_Core_Block</class></core></groups>
        </block>
    </global>
    <front>
        <layouts>
            <noRoute>
                <updates>
                    <core>
                        <file>core/noRoute.xml</file>
                    </core>
                </updates>
            </noRoute>
            <default>
                <updates>
                    <page>
                        <file>core/default.xml</file>
                    </page>
                </updates>
            </default>
        </layouts>
        <routers>
         <core>
            <use>standard</use>
            <args>
               <module>Mage_Core</module>
               <frontName>core</frontName>
            </args>
         </core>
        </routers>
        <default>
            <router>core</router>
        </default>
    </front>
    <translate></translate>
</config>