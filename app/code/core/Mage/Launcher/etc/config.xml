<config>
    <modules>
        <Mage_Launcher>
            <version>2.0.0.0</version>
            <active>true</active>
            <codePool>core</codePool>
        </Mage_Launcher>
    </modules>
    <adminhtml>
        <layout>
            <updates>
                <launcher module="Mage_Launcher">
                    <file>layout.xml</file>
                </launcher>
            </updates>
        </layout>
    </adminhtml>
    <admin>
        <routers>
            <launcher>
                <use>admin</use>
                <args>
                    <module>Mage_Launcher</module>
                    <frontName>launcher</frontName>
                </args>
            </launcher>
        </routers>
    </admin>
    <global>
        <resources>
            <launcher_setup>
                <setup>
                    <module>Mage_Launcher</module>
                    <class>Mage_Launcher_Model_Resource_Setup</class>
                </setup>
            </launcher_setup>
        </resources>
    </global>
    <default>
        <design>
            <head>
                <demonotice>1</demonotice>
            </head>
        </design>
    </default>
    <adminhtml>
        <events>
            <admin_system_config_section_save_after>
                <observers>
                    <launcher_system_config_observer>
                        <class>Mage_Launcher_Model_SystemConfigObserver</class>
                        <method>handleSystemConfigChange</method>
                    </launcher_system_config_observer>
                </observers>
            </admin_system_config_section_save_after>
            <tax_rule_save_commit_after>
                <observers>
                    <launcher_storelauncher_tax_observer>
                        <class>Mage_Launcher_Model_Storelauncher_Tax_SystemObserver</class>
                        <method>handleTaxRuleSave</method>
                    </launcher_storelauncher_tax_observer>
                </observers>
            </tax_rule_save_commit_after>
        </events>
        <launcher>
            <pages>
                <store_launcher>
                    <tiles>
                        <business_info>
                            <state_resolver>Mage_Launcher_Model_Storelauncher_Businessinfo_StateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Storelauncher_Businessinfo_SaveHandler</save_handler>
                        </business_info>
                        <tax>
                            <state_resolver>Mage_Launcher_Model_Storelauncher_Tax_StateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Storelauncher_Tax_SaveHandler</save_handler>
                        </tax>
                        <payments>
                            <state_resolver>Mage_Launcher_Model_Storelauncher_Payments_StateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Storelauncher_Payments_SaveHandler</save_handler>
                        </payments>
                        <shipping>
                            <state_resolver>Mage_Launcher_Model_Storelauncher_Shipping_StateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Storelauncher_Shipping_SaveHandler</save_handler>
                        </shipping>
                        <product>
                            <state_resolver>Mage_Launcher_Model_Storelauncher_Product_StateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Storelauncher_Product_SaveHandler</save_handler>
                        </product>
                    </tiles>
                </store_launcher>
                <promote_store>
                    <tiles>
                        <content_pages>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </content_pages>
                        <reports>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </reports>
                        <seo>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </seo>
                        <home_page>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </home_page>
                        <promotion>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </promotion>
                        <catalog_price_rule>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </catalog_price_rule>
                        <rss>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </rss>
                        <wishlist>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </wishlist>
                        <customer_communication>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </customer_communication>
                        <ebay>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </ebay>
                        <related_products>
                            <state_resolver>Mage_Launcher_Model_Tile_MinimalStateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Tile_MinimalSaveHandler</save_handler>
                        </related_products>
                        <google_analytics>
                            <state_resolver>Mage_Launcher_Model_Promotestore_Googleanalytics_StateResolver</state_resolver>
                            <save_handler>Mage_Launcher_Model_Promotestore_Googleanalytics_SaveHandler</save_handler>
                        </google_analytics>
                    </tiles>
                </promote_store>
            </pages>
        </launcher>
    </adminhtml>
</config>
