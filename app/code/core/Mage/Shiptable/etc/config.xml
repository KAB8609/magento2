<?xml version="1.0"?>
<config>
  <modules>
    <Mage_Shiptable>
      <version>0.1.0</version>
      <load>
        <all>
          <resourceModels>
            <shiptable>
              <class>Mage_Shiptable_Resource_Model_Mysql4</class>
              <entities>
                <shiptable>
                  <table>shiptable_data</table>
                </shiptable>
              </entities>
            </shiptable>
          </resourceModels>
          <resources>
            <shiptable_setup>
              <connection use="core_setup" />
            </shiptable_setup>
            <shiptable_write>
              <connection use="core_write" />
            </shiptable_write>
            <shiptable_read>
              <connection use="core_read" />
            </shiptable_read>
          </resources>
          <salesShippingVendors>
            <shiptable>
              <active>true</active>
              <class>Mage_Shiptable_Table</class>
              <defaults>
                <title>Table rate</title>
              </defaults>
            </shiptable>
          </salesShippingVendors>
        </all>
        <admin>
          <controller>
            <active>true</active>
          </controller>
        </admin>
      </load>
    </Mage_Shiptable>
  </modules>
</config>