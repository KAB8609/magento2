<config>
    <global>
        <resources>
            <googleoptimizer_setup>
                <setup>
                    <module>Mage_GoogleOptimizer</module>
                </setup>
            </googleoptimizer_setup>
        </resources>
    </global>
    <frontend>
        <translate>
            <modules>
                <Mage_GoogleOptimizer>
                    <files>
                        <default>Mage_GoogleOptimizer.csv</default>
                    </files>
                </Mage_GoogleOptimizer>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <Mage_GoogleOptimizer>
                    <files>
                        <default>Mage_GoogleOptimizer.csv</default>
                    </files>
                </Mage_GoogleOptimizer>
            </modules>
        </translate>
    </adminhtml>
    <default>
        <google>
            <optimizer>
                <active>0</active>
            </optimizer>
        </google>
    </default>
</config>