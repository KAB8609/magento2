<config>
    <global>
        <resources>
            <tag_setup>
                <setup>
                    <module>Mage_Tag</module>
                </setup>
            </tag_setup>
        </resources>
        <index>
            <indexer>
                <tag_summary>
                    <model>Mage_Tag_Model_Indexer_Summary</model>
                </tag_summary>
            </indexer>
        </index>
        <session>
            <catalog>
                <product>
                    <message_models>
                        <tag>Mage_Tag_Model_Session</tag>
                    </message_models>
                </product>
            </catalog>
        </session>
    </global>
    <frontend>
        <translate>
            <modules>
                <Mage_Tag>
                    <files>
                        <default>Mage_Tag.csv</default>
                    </files>
                </Mage_Tag>
            </modules>
        </translate>
        <secure_url>
            <tag_customer>/tag/customer/</tag_customer>
        </secure_url>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <Mage_Tag>
                    <files>
                        <default>Mage_Tag.csv</default>
                    </files>
                </Mage_Tag>
            </modules>
        </translate>
    </adminhtml>
</config>