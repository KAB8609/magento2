                <block type="Mage_Page_Block_Html_Head_Script" name="prototype-validation-js">
                    <arguments>
                        <file>prototype/validation.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="varien-js-js">
                    <arguments>
                        <file>varien/js.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="mage-adminhtml-hash-js">
                    <arguments>
                        <file>mage/adminhtml/hash.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="mage-adminhtml-events-js">
                    <arguments>
                        <file>mage/adminhtml/events.js</file>
