                    <arguments>
                        <file>jquery/jquery.hoverIntent.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="mage-translate-js">
                    <arguments>
                        <file>mage/translate.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="mage-backend-bootstrap-js">
                    <arguments>
                        <file>mage/backend/bootstrap.js</file>
