                <block type="Mage_Page_Block_Html_Head_Script" name="scriptaculous-builder-js">
                    <arguments>
                        <file>scriptaculous/builder.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="scriptaculous-effects-js">
                    <arguments>
                        <file>scriptaculous/effects.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="scriptaculous-dragdrop-js">
                    <arguments>
                        <file>scriptaculous/dragdrop.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="scriptaculous-controls-js">
                    <arguments>
                        <file>scriptaculous/controls.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="scriptaculous-slider-js">
