<layout version="0.1.0">
    <default>
        <block type="Mage_Adminhtml_Block_Page" name="root" output="1" template="admin/page.phtml">
            <block type="Mage_Adminhtml_Block_Page_Head" name="head" as="head" template="page/head.phtml">
                <action method="setTitle" translate="title">
                    <title>Magento Admin</title>
                </action>
                <block type="Mage_Page_Block_Html_Head_Script" name="jquery-jquery-js">
                    <arguments>
                        <file>jquery/jquery.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="mage-jquery-no-conflict-js">
                    <arguments>
                        <file>mage/jquery-no-conflict.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="jquery-jquery-ui-js">
                    <arguments>
                        <file>jquery/jquery-ui.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="head-load-min-js">
