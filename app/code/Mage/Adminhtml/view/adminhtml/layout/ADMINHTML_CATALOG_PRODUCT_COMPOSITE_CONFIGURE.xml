<layout version="0.1.0">
    <ADMINHTML_CATALOG_PRODUCT_COMPOSITE_CONFIGURE>
        <block type="Mage_Adminhtml_Block_Catalog_Product_Composite_Fieldset" name="product.composite.fieldset" output="1">
            <block type="Mage_Adminhtml_Block_Catalog_Product_Composite_Fieldset_Options"
                   template="catalog/product/composite/fieldset/options.phtml">
                <block type="Mage_Catalog_Block_Product_View_Options_Type_Default" as="default"
                       template="Mage_Adminhtml::catalog/product/composite/fieldset/options/type/default.phtml"/>
                <block type="Mage_Catalog_Block_Product_View_Options_Type_Text" as="text"
                       template="Mage_Adminhtml::catalog/product/composite/fieldset/options/type/text.phtml"/>
                <block type="Mage_Catalog_Block_Product_View_Options_Type_File" as="file"
                       template="Mage_Adminhtml::catalog/product/composite/fieldset/options/type/file.phtml"/>
                <block type="Mage_Catalog_Block_Product_View_Options_Type_Select" as="select"
                       template="Mage_Adminhtml::catalog/product/composite/fieldset/options/type/select.phtml"/>
                <block type="Mage_Catalog_Block_Product_View_Options_Type_Date" as="date"
                       template="Mage_Adminhtml::catalog/product/composite/fieldset/options/type/date.phtml"/>
                <block type="Mage_Core_Block_Template" name="product.composite.fieldset.options.js" as="options_js"
                       template="Mage_Adminhtml::catalog/product/composite/fieldset/options/js.phtml"/>
            </block>
            <block type="Mage_Adminhtml_Block_Catalog_Product_Composite_Fieldset_Qty"
                   name="product.composite.fieldset.qty" template="catalog/product/composite/fieldset/qty.phtml"/>
        </block>
    </ADMINHTML_CATALOG_PRODUCT_COMPOSITE_CONFIGURE>
</layout>
