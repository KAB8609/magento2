<layout version="0.1.0">
    <page_calendar>
        <reference name="head">
            <block type="Mage_Page_Block_Html_Head_Script" name="jquery-jquery-ui-timepicker-addon-js">
                <arguments>
                    <file>jquery/jquery-ui-timepicker-addon.js</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Script" name="mage-calendar-js">
                <arguments>
                    <file>mage/calendar.js</file>
                </arguments>
            </block>
        </reference>
        <reference name="content">
            <block type="Mage_Core_Block_Html_Calendar" name="html_calendar" as="html_calendar" template="Mage_Page::js/calendar.phtml"/>
        </reference>
    </page_calendar>
</layout>
