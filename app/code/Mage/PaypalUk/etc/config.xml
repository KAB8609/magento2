<config>
    <global>
        <resources>
            <paypaluk_setup>
                <setup>
                    <module>Mage_PaypalUk</module>
                </setup>
            </paypaluk_setup>
        </resources>
    </global>
    <frontend>
        <secure_url>
            <paypaluk_express>/paypaluk/express</paypaluk_express>
        </secure_url>
        <translate>
            <modules>
                <Mage_PaypalUk>
                    <files>
                        <default>Mage_PaypalUk.csv</default>
                    </files>
                </Mage_PaypalUk>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <Mage_PaypalUk>
                    <files>
                        <default>Mage_PaypalUk.csv</default>
                    </files>
                </Mage_PaypalUk>
            </modules>
        </translate>
    </adminhtml>
    <default>
        <payment>
            <paypaluk_express>
                <model>Mage_PaypalUk_Model_Express</model>
            </paypaluk_express>
            <paypaluk_direct>
                <model>Mage_PaypalUk_Model_Direct</model>
            </paypaluk_direct>
        </payment>
    </default>
</config>