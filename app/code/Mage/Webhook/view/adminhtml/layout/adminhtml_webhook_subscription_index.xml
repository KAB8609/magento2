<layout version="0.1.0">
    <adminhtml_webhook_subscription_index>
        <reference name="head">
            <block type="Mage_Page_Block_Html_Head_Css" name="mage-webhook-css-modal-css">
                <arguments>
                    <file>Mage_Webhook::css/modal.css</file>
                </arguments>
            </block>
            <block type="Mage_Page_Block_Html_Head_Script" name="mage-webhook-js-webhook-js">
                <arguments>
                    <file>Mage_Webhook::js/webhook.js</file>
                </arguments>
            </block>
        </reference>
        <reference name="content">
            <block type="Mage_Webhook_Block_Adminhtml_Subscription" name="webhook.subscriptions.list"/>
        </reference>
    </adminhtml_webhook_subscription_index>
</layout>
