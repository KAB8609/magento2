<layout version="0.1.0">
    <adminhtml_webhook_registration_failed>
        <block type="Mage_Page_Block_Html" name="root" output="1" template="empty.phtml">
            <block type="Mage_Adminhtml_Block_Page_Head" name="head" as="head" template="page/head.phtml">
                <block type="Mage_Page_Block_Html_Head_Css" name="mui-reset-css">
                    <arguments>
                        <file>mui/reset.css</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Css" name="mage-webhook-css-boxes-css">
                    <arguments>
                        <file>Mage_Webhook::css/boxes.css</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Css" name="mage-webhook-css-webhook-css">
                    <arguments>
                        <file>Mage_Webhook::css/webhook.css</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="prototype-prototype-js">
                    <arguments>
                        <file>prototype/prototype.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="mage-translate-js">
                    <arguments>
                        <file>mage/translate.js</file>
                    </arguments>
                </block>
                <block type="Mage_Page_Block_Html_Head_Script" name="mage-adminhtml-tools-js">
                    <arguments>
                        <file>mage/adminhtml/tools.js</file>
                    </arguments>
                </block>
            </block>
            <block type="Mage_Webhook_Block_Adminhtml_Registration_Failed" name="content" template="Mage_Webhook::registration/failed.phtml"/>
        </block>
    </adminhtml_webhook_registration_failed>
</layout>
