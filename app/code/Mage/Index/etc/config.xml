<config>
    <global>
        <resources>
            <index_setup>
                <setup>
                    <module>Mage_Index</module>
                    <class>Mage_Index_Model_Resource_Setup</class>
                </setup>
            </index_setup>
        </resources>
        <index>
            <indexer />
        </index>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Mage_Index before="Mage_Adminhtml">Mage_Index_Adminhtml</Mage_Index>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <translate>
            <modules>
                <Mage_Index>
                    <files>
                        <default>Mage_Index.csv</default>
                    </files>
                </Mage_Index>
            </modules>
        </translate>
    </adminhtml>
</config>