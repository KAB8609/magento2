<config>
    <global>
        <resources>
            <designeditor_setup>
                <setup>
                    <module>Mage_DesignEditor</module>
                    <class>Mage_Core_Model_Resource_Setup</class>
                </setup>
            </designeditor_setup>
        </resources>
    </global>
    <default>
        <theme>
            <customization>
                <quick_style_css>Mage_DesignEditor_Model_Theme_Customization_File_QuickStyleCss</quick_style_css>
            </customization>
        </theme>
    </default>
    <vde>
        <design_editor>
            <frontName>vde</frontName>
            <disabledCacheTypes>
                <block_html />
                <config />
            </disabledCacheTypes>
        </design_editor>
    </vde>
</config>
