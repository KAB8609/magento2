<config>
    <services>
        <catalog_category translate="title" module="Mage_Catalog">
            <title>Catalog Category Service</title>
            <class>Mage_Catalog_Service_CategoryEntity</class>
            <methods>
                <create>
                    <title>Create new category</title>
                </create>
                <item>
                    <title>Retrieve a category</title>
                </item>
                <items>
                    <title>Retrieve category collection by filters</title>
                </items>
