<layout version="0.1.0">
    <default>
        <reference name="head">
            <block type="Mage_Page_Block_Html_Head_Css" name="mage-reports-widgets-css">
                <arguments>
                    <file>Mage_Reports::widgets.css</file>
                </arguments>
            </block>
        </reference>
        <reference name="right">
            <block type="Mage_Reports_Block_Product_Viewed" before="right.permanent.callout" name="right.reports.product.viewed" template="product_viewed.phtml"/>
            <block type="Mage_Reports_Block_Product_Compared" before="right.permanent.callout" name="right.reports.product.compared" template="product_compared.phtml"/>
        </reference>
    </default>
</layout>
