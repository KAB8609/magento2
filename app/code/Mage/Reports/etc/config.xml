<config>
    <global>
        <resources>
            <reports_setup>
                <setup>
                    <module>Mage_Reports</module>
                </setup>
            </reports_setup>
        </resources>
    </global>
    <adminhtml>
        <translate>
            <modules>
                <Mage_Reports>
                    <files>
                        <default>Mage_Reports.csv</default>
                    </files>
                </Mage_Reports>
            </modules>
        </translate>
    </adminhtml>
    <frontend>
        <translate>
            <modules>
                <Mage_Reports>
                    <files>
                        <default>Mage_Reports.csv</default>
                    </files>
                </Mage_Reports>
            </modules>
        </translate>
    </frontend>
    <default>
        <catalog>
            <recently_products>
                <scope>website</scope>
                <viewed_count>5</viewed_count>
                <compared_count>5</compared_count>
            </recently_products>
        </catalog>
        <reports>
            <dashboard>
                <ytd_start>1,1</ytd_start>
                <mtd_start>1</mtd_start>
            </dashboard>
        </reports>
    </default>
</config>