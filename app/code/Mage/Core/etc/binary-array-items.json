{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "title": "Binary Operator Array Item Expressions",
    "description": "A Json Schema for validating item expressions in a binary operator array",
    "type": "array",
    "items": {
        "type": "object",
        "patternProperties": {
            "^[a-zA-z][a-zA-Z0-9\\-_\\.]*$": {"$ref": "unary-expressions.json"}
        },
        "additionalProperties": false
    },
    "minItems": 2,
    "additionalItems": false
}

