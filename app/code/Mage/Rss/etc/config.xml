<config>
    <modules>
        <Mage_Rss>
            <version>1.6.0.0</version>
            <active>true</active>
            <depends>
                <Mage_Core/>
                <Mage_Customer/>
                <Mage_Wishlist/>
                <Mage_Sales/>
                <Mage_Backend/>
                <Mage_Catalog/>
                <Mage_Review/>
                <Mage_Adminhtml/>
                <Mage_CatalogInventory/>
                <Mage_SalesRule/>
                <Mage_GiftMessage/>
                <Mage_Page/>
            </depends>
            <sequence>
                <Mage_Catalog/>
                <Mage_CatalogInventory/>
                <Mage_Sales/>
                <Mage_SalesRule/>
                <Mage_Wishlist/>
            </sequence>
        </Mage_Rss>
    </modules>
    <admin>

    </admin>
    <adminhtml>
        <translate>
            <modules>
                <Mage_Rss>
                    <files>
                        <default>Mage_Rss.csv</default>
                    </files>
                </Mage_Rss>
            </modules>
        </translate>
    </adminhtml>
    <frontend>
        <translate>
            <modules>
                <Mage_Rss>
                    <files>
                        <default>Mage_Rss.csv</default>
                    </files>
                </Mage_Rss>
            </modules>
        </translate>
        <secure_url>
            <rss_catalog_review>/rss/catalog/review</rss_catalog_review>
            <rss_order_new>/rss/order/new</rss_order_new>
            <rss_catalog_notifystock>/rss/catalog/notifystock</rss_catalog_notifystock>
        </secure_url>
        <routers>
            <rss>
                <use>standard</use>
                <args>
                    <module>Mage_Rss</module>
                    <frontName>rss</frontName>
                </args>
            </rss>
        </routers>
    </frontend>
</config>
