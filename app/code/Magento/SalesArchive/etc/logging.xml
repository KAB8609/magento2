<logging xsi:noNamespaceSchemaLocation="../../../Magento/Logging/etc/logging.xsd"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <title action="massAdd" translate="true" >Mass Add</title >
    <title action="add" translate="true" >Add</title >
    <event id="magento_salesarchive" >
        <label translate="true" >Sales Archive</label >
        <expected_model class="Magento_SalesArchive_Model_Archive" />
        <handle name="adminhtml_sales_archive_add" action="add" skip_on_back="adminhtml_sales_order_view "
                post_dispatch="postDispatchSalesArchiveManagement" />
        <handle name="adminhtml_sales_archive_remove" action="delete" skip_on_back="adminhtml_sales_order_view "
                post_dispatch="postDispatchSalesArchiveManagement" />
        <handle name="adminhtml_sales_archive_massAdd" action="massAdd"
                post_dispatch="postDispatchSalesArchiveManagement" />
        <handle name="adminhtml_sales_archive_massRemove" action="massDelete"
                post_dispatch="postDispatchSalesArchiveManagement" />
    </event >
</logging >