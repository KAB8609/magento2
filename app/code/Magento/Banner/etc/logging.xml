<logging xsi:noNamespaceSchemaLocation="../../../Magento/Logging/etc/logging.xsd"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <event id="magento_banner" >
        <label translate="true" >Manage Banners</label >
        <expected_model class="Magento_Banner_Model_Banner" />
        <handle name="adminhtml_banner_edit" action="view" />
        <handle name="adminhtml_banner_save" action="save" skip_on_back="adminhtml_banner_edit " />
        <handle name="adminhtml_banner_delete" action="delete" />
        <handle name="adminhtml_banner_massDelete" action="massDelete" />
    </event >
</logging >