<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle label="CMS Pages (All)" id="cms_page_view" type="page" parent="default">
        <reference name="content">
            <block class="Magento_Core_Block_Template" name="page_content_heading" template="Magento_Cms::content_heading.phtml"/>
            <container name="cms.wrapper" label="CMS Content Wrapper" htmlTag="div" htmlClass="std">
                <block class="Magento_Cms_Block_Page" name="cms_page"/>
            </container>
        </reference>
    </handle>
</layout>
