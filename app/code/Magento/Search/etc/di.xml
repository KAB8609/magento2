<config>
    <preference for="Magento\Search\Helper\ClientInterface" type="Magento\Search\Helper\Data" />
    <type name="Magento\CatalogSearch\Model\Fulltext">
        <plugin name="fulltextIndexRebuild" type="Magento\Search\Model\Plugin\FulltextIndexRebuild" />
    </type>
    <type name="Magento_Search_Block_Suggestions">
        <param name="suggestions">
            <instance type="Magento_Search_Model_Suggestions_Proxy" />
        </param>
    </type>
    <type name="Magento_Search_Block_Catalog_Layer_View">
        <param name="searchLayer">
            <instance type="Magento_Search_Model_Search_Layer_Proxy" />
        </param>
    </type>
    <type name="Magento_Search_Block_Catalogsearch_Layer">
        <param name="searchLayer">
            <instance type="Magento_Search_Model_Search_Layer_Proxy" />
        </param>
    </type>
    <type name="Magento_Search_Model_Catalog_Layer_Filter_Price">
        <param name="resourceEngine">
            <instance type="Magento_Search_Model_Resource_Engine_Proxy" />
        </param>
    </type>
    <type name="Magento_Search_Model_Observer">
        <param name="searchSearchLayer">
            <instance type="Magento_Search_Model_Search_Layer_Proxy" />
        </param>
        <param name="searchCatalogLayer">
            <instance type="Magento_Search_Model_Catalog_Layer_Proxy" />
        </param>
        <param name="indexer">
            <instance type="Magento_Index_Model_Indexer_Proxy" />
        </param>
        <param name="searchData">
            <instance type="Magento_Search_Helper_Data_Proxy" />
        </param>
        <param name="coreRegistry">
            <instance type="Magento_Core_Model_Registry_Proxy" />
        </param>
    </type>
</config>
