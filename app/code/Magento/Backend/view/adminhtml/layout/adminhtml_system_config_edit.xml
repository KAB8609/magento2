<layout version="0.1.0">
    <adminhtml_system_config_edit>
        <reference name="head">
            <action method="addJs">
                <file>jquery/jquery.ba-hashchange.min.js</file>
            </action>
        </reference>
        <reference name="js">
            <block type="Magento_Adminhtml_Block_System_Shipping_Ups" template="Magento_Adminhtml::system/shipping/ups.phtml"/>
            <block type="Magento_Backend_Block_Template" template="system/config/js.phtml"/>
            <block type="Magento_Backend_Block_Template" template="Magento_Adminhtml::system/shipping/applicable_country.phtml"/>
        </reference>
        <reference name="left">
            <block type="Magento_Backend_Block_System_Config_Tabs" name="adminhtml.system.config.tabs" before="-"/>
            <block type="Magento_Backend_Block_System_Config_Switcher" name="adminhtml.system.config.switcher" before="-"/>
        </reference>
        <reference name="content">
            <block type="Magento_Backend_Block_System_Config_Edit"/>
        </reference>
    </adminhtml_system_config_edit>
</layout>
