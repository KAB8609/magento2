<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="adminhtml_webhook_registration_succeeded">
        <block class="Magento\Page\Block\Html" name="root" output="1" template="empty.phtml">
            <block class="Magento\Adminhtml\Block\Page\Head" name="head" as="head" template="page/head.phtml">
                <action method="addCss">
                    <argument name="file" xsi:type="string">mui/reset.css</argument>
                </action>
                <action method="addCss">
                    <argument name="file" xsi:type="string">Magento_Webhook::css/boxes.css</argument>
                </action>
                <action method="addCss">
                    <argument name="file" xsi:type="string">Magento_Webhook::css/webhook.css</argument>
                </action>
                <action method="addJs">
                    <argument name="file" xsi:type="string">jquery/jquery.js</argument>
                </action>
                <action method="addJs">
                    <argument name="script" xsi:type="string">prototype/prototype.js</argument>
                </action>
                <action method="addJs">
                    <argument name="script" xsi:type="string">mage/translate.js</argument>
                </action>
                <action method="addJs">
                    <argument name="script" xsi:type="string">mage/adminhtml/tools.js</argument>
                </action>
            </block>
            <block class="Magento\Backend\Block\Template" name="content" template="Magento_Webhook::registration/succeeded.phtml"/>
        </block>
    </handle>
</layout>
