<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="adminhtml_webhook_registration_activate">
        <block class="Magento_Page_Block_Html" name="root" output="1" template="empty.phtml">
            <block class="Magento_Adminhtml_Block_Page_Head" name="head" as="head" template="page/head.phtml">
                <action method="addCss">
                    <file>mui/reset.css</file>
                </action>
                <action method="addCss">
                    <file>Magento_Webhook::css/boxes.css</file>
                </action>
                <action method="addCss">
                    <file>Magento_Webhook::css/webhook.css</file>
                </action>
                <action method="addJs">
                    <file>jquery/jquery.js</file>
                </action>
                <action method="addJs">
                    <script>prototype/prototype.js</script>
                </action>
                <action method="addJs">
                    <script>mage/translate.js</script>
                </action>
                <action method="addJs">
                    <script>mage/adminhtml/tools.js</script>
                </action>
            </block>
            <block class="Magento_Webhook_Block_Adminhtml_Registration_Activate" name="content" template="Magento_Webhook::registration/activate.phtml"/>
        </block>
    </handle>
</layout>
