<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <block class="Magento_Page_Block_Html" name="root" output="1" template="empty.phtml">
        <block class="Magento_Adminhtml_Block_Page_Head" name="head" as="head" template="page/head.phtml">
            <action method="addCss">
                <argument name="file" xsi:type="string">mui/reset.css</argument>
            </action>
            <action method="addCss">
                <argument name="file" xsi:type="string">Magento_Webhook::css/boxes.css</argument>
            </action>
            <action method="addCss">
                <argument name="file" xsi:type="string">Magento_Webhook::css/webhook.css</argument>
            </action>
            <action method="addJs">
                <argument name="file" xsi:type="string">jquery/jquery.js</argument>
            </action>
            <action method="addJs">
                <argument name="script" xsi:type="string">prototype/prototype.js</argument>
            </action>
            <action method="addJs">
                <argument name="script" xsi:type="string">mage/translate.js</argument>
            </action>
            <action method="addJs">
                <argument name="script" xsi:type="string">mage/adminhtml/tools.js</argument>
            </action>
        </block>
        <block class="Magento_Webhook_Block_Adminhtml_Registration_Activate" name="content" template="Magento_Webhook::registration/activate.phtml"/>
    </block>
</layout>
