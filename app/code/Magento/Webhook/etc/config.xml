<config>
    <global>
        <resources>
            <webhook_setup>
                <setup>
                    <module>Magento_Webhook</module>
                </setup>
            </webhook_setup>
        </resources>
    </global>
    <crontab>
        <jobs>
            <webhook_process_messages>
                <schedule><cron_expr>* * * * *</cron_expr></schedule>
                <run><model>\Magento\PubSub\Event\QueueHandler::handle</model></run>
            </webhook_process_messages>
            <webhook_process_dispatchers>
                <schedule><cron_expr>* * * * *</cron_expr></schedule>
                <run><model>\Magento\PubSub\Job\QueueHandler::handle</model></run>
            </webhook_process_dispatchers>
            <webhook_events_revoke_idling>
                <schedule><cron_expr>*/15 * * * *</cron_expr></schedule>
                <run><model>Magento_Webhook_Model_Resource_Event_Collection::revokeIdlingInProgress</model></run>
            </webhook_events_revoke_idling>
            <webhook_jobs_revoke_idling>
                <schedule><cron_expr>*/15 * * * *</cron_expr></schedule>
                <run><model>Magento_Webhook_Model_Resource_Job_Collection::revokeIdlingInProgress</model></run>
            </webhook_jobs_revoke_idling>
        </jobs>
    </crontab>
</config>
