<config>
    <service class="Magento\Webhook\Service\SubscriptionV1Interface" baseUrl="/V1/subscriptions">
        <rest-route httpMethod="GET" method="getAll" resources="Magento_Webhook::webhook" isSecure="true"/>
        <rest-route httpMethod="POST" method="create" resources="Magento_Webhook::webhook" isSecure="true"/>
        <rest-route httpMethod="PUT" method="update" resources="Magento_Webhook::webhook" isSecure="true">/:subscriptionId</rest-route>
        <rest-route httpMethod="GET" method="get" resources="Magento_Webhook::webhook" isSecure="true">/:subscriptionId</rest-route>
        <rest-route httpMethod="DELETE" method="delete" resources="Magento_Webhook::webhook" isSecure="true">/:subscriptionId</rest-route>
    </service>
</config>
