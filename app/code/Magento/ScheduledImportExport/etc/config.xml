<config>
    <global>
        <template>
            <email>
                <magento_scheduledimportexport_import_failed translate="label" module="Magento_ScheduledImportExport">
                    <label>Import Failed</label>
                    <file>email/import_failed.html</file>
                    <type>html</type>
                </magento_scheduledimportexport_import_failed>
                <magento_scheduledimportexport_export_failed translate="label" module="Magento_ScheduledImportExport">
                    <label>Export Failed</label>
                    <file>email/export_failed.html</file>
                    <type>html</type>
                </magento_scheduledimportexport_export_failed>
                <system_magento_scheduled_import_export_log_error_email_template translate="label" module="Magento_ScheduledImportExport">
                    <label>File History Clean Failed</label>
                    <file>email/file_history_clean_failed.html</file>
                    <type>html</type>
                </system_magento_scheduled_import_export_log_error_email_template>
            </email>
        </template>
    </global>
    <default>
        <system>
            <magento_scheduled_import_export_log>
                <save_days>5</save_days>
                <error_email_template>system_magento_scheduled_import_export_log_error_email_template</error_email_template>
            </magento_scheduled_import_export_log>
        </system>
        <export>
            <customer_page_size>
                <finance>10000</finance>
            </customer_page_size>
        </export>
    </default>
</config>
