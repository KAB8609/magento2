<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <update handle="adminhtml_report_customer_wishlist_block"/>
    <referenceContainer name="content">
        <block class="Magento_MultipleWishlist_Block_Adminhtml_Report_Customer_Wishlist" name="adminhtml.block.report.customer.wishlist.container" template="Magento_Adminhtml::report/grid/container.phtml">
            <block class="Magento_Backend_Block_Store_Switcher" template="Magento_Reports::store/switcher/enhanced.phtml" name="store.switcher">
                <action method="setStoreVarName">
                    <argument name="var_name" xsi:type="string">store_ids</argument>
