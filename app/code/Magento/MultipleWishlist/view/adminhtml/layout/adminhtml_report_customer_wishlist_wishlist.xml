<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="adminhtml_report_customer_wishlist_wishlist">
        <update handle="adminhtml_report_customer_wishlist_block"/>
        <reference name="content">
            <block class="Magento\MultipleWishlist\Block\Adminhtml\Report\Customer\Wishlist" name="adminhtml.block.report.customer.wishlist.container" template="Magento_Adminhtml::report/grid/container.phtml">
                <block class="Magento\Backend\Block\Store\Switcher" template="Magento_Reports::store/switcher/enhanced.phtml" name="store.switcher">
                    <action method="setStoreVarName">
                        <argument name="var_name" xsi:type="string">store_ids</argument>
                    </action>
                </block>
            </block>
        </reference>
    </handle>
</layout>
