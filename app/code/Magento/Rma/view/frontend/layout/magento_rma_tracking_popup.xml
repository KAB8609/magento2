<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="magento_rma_tracking_popup" translate="label" type="page" parent="magento_rma_return_view">
        <label>Shipment Tracking Popup</label>
        <reference name="root">
            <action method="setTemplate">
                <template>popup.phtml</template>
            </action>
        </reference>
        <reference name="content">
            <block class="Magento_Rma_Block_Return_Tracking_Popup" name="rma.tracking.popup" template="Magento_Shipping::tracking/popup.phtml"/>
        </reference>
    </handle>
</layout>
