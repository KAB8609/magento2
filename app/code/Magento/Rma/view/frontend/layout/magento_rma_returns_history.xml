<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" label="Customer My Returns History" type="page" parent="magento_rma_returns_view">
    <update handle="customer_account"/>
    <referenceBlock name="customer_account_navigation">
        <action method="setActive">
            <argument name="path" xsi:type="string">rma/returns/history/</argument>
        </action>
    </referenceBlock>
    <referenceContainer name="my.account.wrapper">
        <block class="Magento\Rma\Block\Returns\History" name="rma.returns.history">
            <container name="rma.returns.history.info" as="info" label="Returns History Info"/>
        </block>
        <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="Magento_Customer::account/link/back.phtml"/>
    </referenceContainer>
</layout>
