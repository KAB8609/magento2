<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" label="Shipment Print Package Popup" type="page" parent="magento_rma_tracking_package">
    <reference name="root">
        <action method="setTemplate">
            <argument name="template" xsi:type="string">popup.phtml</argument>
        </action>
    </reference>
    <reference name="content">
        <block class="Magento\Rma\Block\Returns\Tracking\Package" name="rma.tracking.popup" template="return/popup/package.phtml"/>
    </reference>
</layout>
