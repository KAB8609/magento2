<layout version="0.1.0">
    <magento_rma_tracking_printlabel translate="label" type="page" parent="magento_rma_tracking_package">
        <label>Shipment Print Label Popup</label>
        <reference name="root">
            <action method="setTemplate">
                <template>popup.phtml</template>
            </action>
        </reference>
        <reference name="content">
            <block type="Magento_Rma_Block_Return_Tracking_Package" name="rma.tracking.popup" template="return/popup/package.phtml"/>
        </reference>
    </magento_rma_tracking_printlabel>
</layout>
