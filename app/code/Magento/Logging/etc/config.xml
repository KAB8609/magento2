<config>
    <global>
        <resources>
            <logging_setup>
                <setup>
                    <module>Magento_Logging</module>
                    <class>Magento\Core\Model\Resource\Setup</class>
                </setup>
            </logging_setup>
        </resources>
    </global>

    <default>
        <system>
            <rotation translate="frequency">
                <frequency>1</frequency>
                <lifetime>60</lifetime>
            </rotation>
        </system>
    </default>
</config>
