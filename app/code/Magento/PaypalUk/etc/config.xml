<config>
    <global>
        <resources>
            <paypaluk_setup>
                <setup>
                    <module>Magento_PaypalUk</module>
                </setup>
            </paypaluk_setup>
        </resources>
    </global>
    <frontend>
        <secure_url>
            <paypaluk_express>/paypaluk/express</paypaluk_express>
        </secure_url>
    </frontend>
    <default>
        <payment>
            <paypaluk_express>
                <model>Magento\PaypalUk\Model\Express</model>
            </paypaluk_express>
            <paypaluk_direct>
                <model>Magento\PaypalUk\Model\Direct</model>
            </paypaluk_direct>
        </payment>
    </default>
</config>
