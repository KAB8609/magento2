<logging xsi:noNamespaceSchemaLocation="../../../Magento/Logging/etc/logging.xsd"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <event id="magento_catalogevent" >
        <label translate="true" >Catalog Events</label >
        <expected_model class="Magento_CatalogEvent_Model_Event" />
        <handle name="adminhtml_catalog_event_edit" action="view" />
        <handle name="adminhtml_catalog_event_save" action="save" skip_on_back="adminhtml_catalog_event_edit " />
        <handle name="adminhtml_catalog_event_delete" action="delete" />
    </event >
</logging >