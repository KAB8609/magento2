<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <reference name="creditmemo_items">
        <action method="addItemRender">
            <argument name="type" xsi:type="string">downloadable</argument>
            <argument name="block" xsi:type="string">Magento_Adminhtml_Block_Sales_Items_Renderer_Default</argument>
            <argument name="template" xsi:type="string">Magento_Downloadable::sales/order/creditmemo/view/items/renderer/downloadable.phtml</argument>
        </action>
        <action method="addColumnRender">
            <argument name="column" xsi:type="string">downloadable</argument>
            <argument name="block" xsi:type="string">Magento_Downloadable_Block_Adminhtml_Sales_Items_Column_Downloadable_Name</argument>
            <argument name="template" xsi:type="string">sales/items/column/downloadable/creditmemo/name.phtml</argument>
        </action>
    </reference>
</layout>
