<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="checkout_multishipping_shipping">
        <reference name="checkout_billing_items">
            <action method="addItemRender">
                <argument name="type" xsi:type="string">downloadable</argument>
                <argument name="block" xsi:type="string">\Magento\Downloadable\Block\Checkout\Cart\Item\Renderer</argument>
                <argument name="template" xsi:type="string">checkout/multishipping/item/downloadable.phtml</argument>
            </action>
        </reference>
    </handle>
</layout>
