<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="page_calendar">
        <reference name="head">
            <action method="addJs">
                <file>jquery/jquery-ui-timepicker-addon.js</file>
                <!--<if/><condition>can_load_calendar_js</condition>-->
            </action>
            <action method="addJs">
                <file>mage/calendar.js</file>
                <!--<if/><condition>can_load_calendar_js</condition>-->
            </action>
        </reference>
        <reference name="content">
            <block class="Magento_Core_Block_Html_Calendar" name="html_calendar" as="html_calendar" template="Magento_Page::js/calendar.phtml"/>
        </reference>
    </handle>
</layout>
