<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="customer_account">
        <block class="Magento_AdvancedCheckout_Block_Customer_Link" name="customer_link_sku">
            <action method="addDashboardLink" translate="label">
                <argument name="block" xsi:type="string">customer_account_navigation</argument>
                <argument name="name" xsi:type="string">checkout</argument>
                <argument name="path" xsi:type="string">enterprise_checkout/sku</argument>
                <argument name="label" xsi:type="string">Order by SKU</argument>
            </action>
        </block>
    </handle>
</layout>
