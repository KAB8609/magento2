<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="customer_account">
        <block class="\Magento\AdvancedCheckout\Block\Customer\Link" name="customer_link_sku">
            <action method="addDashboardLink">
                <argument name="block" xsi:type="string">customer_account_navigation</argument>
                <argument name="name" xsi:type="string">checkout</argument>
                <argument name="path" xsi:type="string">magento_advancedcheckout/sku</argument>
                <argument translate="true" name="label" xsi:type="string">Order by SKU</argument>
            </action>
        </block>
    </handle>
</layout>
