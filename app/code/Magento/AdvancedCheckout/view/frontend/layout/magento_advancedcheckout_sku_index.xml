<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="magento_advancedcheckout_sku_index" translate="label" type="page" parent="checkout_cart_index">
        <label>Customer Checkout by SKU</label>
        <update handle="customer_account"/>
        <reference name="my.account.wrapper">
            <block class="Magento_Core_Block_Template" name="checkout.customer.order" template="Magento_AdvancedCheckout::customer/sku.phtml">
                <block class="Magento_AdvancedCheckout_Block_Customer_Sku" name="checkout.customer.sku" template="widget/sku.phtml">
                    <action method="setIsTitleHidden">
                        <argument name="value" xsi:type="string">true</argument>
                    </action>
                    <block class="Magento_Core_Block_Template" template="Magento_AdvancedCheckout::customer/file.phtml"/>
                </block>
            </block>
        </reference>
    </handle>
</layout>
