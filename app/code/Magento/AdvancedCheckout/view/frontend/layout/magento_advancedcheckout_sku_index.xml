<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle label="Customer Checkout by SKU" id="magento_advancedcheckout_sku_index" type="page" parent="checkout_cart_index">
        <update handle="customer_account"/>
        <reference name="my.account.wrapper">
            <block class="\Magento\Core\Block\Template" name="checkout.customer.order" template="Magento_AdvancedCheckout::customer/sku.phtml">
                <block class="\Magento\AdvancedCheckout\Block\Customer\Sku" name="checkout.customer.sku" template="widget/sku.phtml">
                    <action method="setIsTitleHidden">
                        <argument name="value" xsi:type="string">true</argument>
                    </action>
                    <block class="\Magento\Core\Block\Template" template="Magento_AdvancedCheckout::customer/file.phtml"/>
                </block>
            </block>
        </reference>
    </handle>
</layout>
