<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="sku_layout_update">
        <update handle="adminhtml_sku_errors_block"/>
        <reference name="head">
            <action method="addJs">
                <argument name="file" xsi:type="string">Magento_Adminhtml::catalog/product/composite/configure.js</argument>
            </action>
        </reference>
        <reference name="data">
            <block class="Magento\AdvancedCheckout\Block\Adminhtml\Sales\Order\Create\Sku" name="additional_area" template="Magento_Adminhtml::sales/order/create/abstract.phtml">
                <block class="Magento\AdvancedCheckout\Block\Adminhtml\Sales\Order\Create\Sku\Add"/>
            </block>
            <block class="Magento\AdvancedCheckout\Block\Adminhtml\Sales\Order\Create\Sku\Errors" name="errors"/>
        </reference>
    </handle>
</layout>
