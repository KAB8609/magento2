<config>
    <preference for="Magento\AdvancedCheckout\Model\Cart" type="Magento\AdvancedCheckout\Model\Backend\Cart" />
    <type name="Magento\AdvancedCheckout\Model\Backend\Cart">
        <param name="itemStatusFailed">
            <value type="const">Magento\AdvancedCheckout\Helper\Data::ADD_ITEM_STATUS_FAILED_WEBSITE</value>
        </param>
    </type>
    <type name="Magento\AdvancedCheckout\Helper\Data">
        <param name="cart">
            <instance type="Magento\AdvancedCheckout\Model\Cart\Proxy" />
        </param>
        <param name="url">
            <instance type="Magento\Core\Model\Url\Proxy" />
        </param>
        <param name="catalogConfig">
            <instance type="Magento\Catalog\Model\Config\Proxy" />
        </param>
        <param name="session">
            <instance type="Magento\Customer\Model\Session\Proxy" />
        </param>
        <param name="customerSession">
            <instance type="Magento\Customer\Model\Session\Proxy" />
        </param>
        <param name="checkoutSession">
            <instance type="Magento\Checkout\Model\Session\Proxy" />
        </param>
        <param name="coreStoreConfig">
            <instance type="Magento\Core\Model\Store\Config\Proxy" />
        </param>
    </type>
</config>
