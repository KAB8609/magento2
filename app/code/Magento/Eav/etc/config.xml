<config>
    <global>
        <resources>
            <eav_setup>
                <setup>
                    <module>Magento_Eav</module>
                    <class>Magento_Eav_Model_Entity_Setup</class>
                </setup>
            </eav_setup>
        </resources>
        <eav_frontendclasses />
        <eav_attributes />
    </global>
    <adminhtml>
        <translate>
            <modules>
                <Magento_Eav>
                    <files>
                        <default>Magento_Eav.csv</default>
                    </files>
                </Magento_Eav>
            </modules>
        </translate>
    </adminhtml>
    <frontend>
        <translate>
            <modules>
                <Magento_Eav>
                    <files>
                        <default>Magento_Eav.csv</default>
                    </files>
                </Magento_Eav>
            </modules>
        </translate>
    </frontend>
    <default>
        <general>
            <validator_data>
                <input_types>
                    <text>text</text>
                    <textarea>textarea</textarea>
                    <date>date</date>
                    <boolean>boolean</boolean>
                    <multiselect>multiselect</multiselect>
                    <select>select</select>
                </input_types>
            </validator_data>
        </general>
    </default>
</config>
