<config>
    <global>
        <resources>
            <eav_setup>
                <setup>
                    <module>Magento_Eav</module>
                    <class>Magento\Eav\Model\Entity\Setup</class>
                </setup>
            </eav_setup>
        </resources>
    </global>
    <default>
        <general>
            <validator_data>
                <input_types>
                    <text>text</text>
                    <textarea>textarea</textarea>
                    <date>date</date>
                    <boolean>boolean</boolean>
                    <multiselect>multiselect</multiselect>
                    <select>select</select>
                </input_types>
            </validator_data>
        </general>
    </default>
</config>
