<config>
    <global>
        <resources>
            <backup_setup>
                <setup>
                    <module>Magento_Backup</module>
                </setup>
            </backup_setup>
        </resources>
    </global>
    <adminhtml>
        <translate>
            <modules>
                <Magento_Backup>
                    <files>
                        <default>Magento_Backup.csv</default>
                    </files>
                </Magento_Backup>
            </modules>
        </translate>
    </adminhtml>
    <crontab>
        <jobs>
            <system_backup>
                <run>
                    <model>Magento_Backup_Model_Observer::scheduledBackup</model>
                </run>
            </system_backup>
        </jobs>
    </crontab>
</config>
