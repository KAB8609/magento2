        <param name="designChange">
            <instance type="Magento\Core\Model\Design\Proxy" />
        </param>
        <param name="design">
            <instance type="Magento\Core\Model\View\Design\Proxy" />
        </param>
    </type>
    <type name="Magento_FullPageCache_Model_Container_Abstract">
        <param name="eventManager">
            <instance type="Magento_Core_Model_Event_Manager_Proxy" />
        </param>
    </type>
