<config>
    <modules>
        <Magento_Media>
            <version>1.6.0.0</version>
            <active>true</active>
            <depends>
                <Magento_Core/>
            </depends>
        </Magento_Media>
    </modules>
    <adminhtml>
        <translate>
            <modules>
                <Magento_Media>
                    <files>
                        <default>Magento_Media.csv</default>
                    </files>
                </Magento_Media>
            </modules>
        </translate>
    </adminhtml>
    <frontend>
        <routers>
            <media>
                <use>standard</use>
                <args>
                    <module>Magento_Media</module>
                    <frontName>media</frontName>
                </args>
            </media>
        </routers>
        <translate>
            <modules>
                <Magento_Media>
                    <files>
                        <default>Magento_Media.csv</default>
                    </files>
                </Magento_Media>
            </modules>
        </translate>
    </frontend>
</config>
