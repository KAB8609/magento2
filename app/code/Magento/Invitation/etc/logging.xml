<logging xsi:noNamespaceSchemaLocation="../../../Magento/Logging/etc/logging.xsd"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <event id="magento_invitation" >
        <label translate="true" >Customer Invitations</label >
        <expected_model class="Magento_Invitation_Model_Invitation" />
        <handle name="adminhtml_invitation_view" action="view" />
        <handle name="adminhtml_invitation_save" action="save" />
        <handle name="adminhtml_invitation_saveInvitation" action="save" />
        <handle name="adminhtml_invitation_massCancel" action="massUpdate" />
        <handle name="adminhtml_invitation_massResend" action="massUpdate"
                post_dispatch="Magento_Invitation_Model_Observer::postDispatchInvitationMassUpdate" />
    </event >
</logging >