    <virtualType name="Magento_Payment_Model_Config_Data" type="Magento_Config_Data">
        <param name='reader'>
            <instance type="Magento_Payment_Model_Config_Reader" />
        </param>
        <param name="cacheId">
            <value>payment_config</value>
        </param>
    </virtualType>
    <type name="Magento_Payment_Model_Config">
        <param name="dataStorage">
            <instance type="Magento_Payment_Model_Config_Data" />
        </param>
    </type>
