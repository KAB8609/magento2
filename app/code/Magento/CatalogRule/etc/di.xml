        <param name="helper">
            <instance type="Magento\CatalogRule\Helper\Data" />
        </param>
    </type>
    <type name="Magento\CatalogRule\Model\Resource\Rule">
        <param name="eventManager">
            <instance type="Magento\Event\ManagerInterface\Proxy" />
