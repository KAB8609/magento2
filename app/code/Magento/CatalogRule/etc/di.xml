        </param>
    </type>
    <type name="Magento\App\Updater\SetupFactory">
        <param name="resourceTypes">
            <value>
                <catalogrule_setup>Magento\Core\Model\Resource\Setup\Generic</catalogrule_setup>
            </value>
        </param>
    </type>
</config>
