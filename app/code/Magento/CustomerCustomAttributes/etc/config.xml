<config>
    <global>
        <resources>
            <customercustomattributes_setup>
                <setup>
                    <module>Magento_CustomerCustomAttributes</module>
                    <class>Magento_CustomerCustomAttributes_Model_Resource_Setup</class>
                </setup>
            </customercustomattributes_setup>
        </resources>
        <events>
            <sales_quote_load_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>salesQuoteAfterLoad</method>
                    </magento_customercustomattributes>
                </observers>
            </sales_quote_load_after>
            <sales_quote_address_collection_load_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>salesQuoteAddressCollectionAfterLoad</method>
                    </magento_customercustomattributes>
                </observers>
            </sales_quote_address_collection_load_after>
            <sales_quote_save_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>salesQuoteAfterSave</method>
                    </magento_customercustomattributes>
                </observers>
            </sales_quote_save_after>
            <sales_quote_address_save_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>salesQuoteAddressAfterSave</method>
                    </magento_customercustomattributes>
                </observers>
            </sales_quote_address_save_after>
            <sales_order_load_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>salesOrderAfterLoad</method>
                    </magento_customercustomattributes>
                </observers>
            </sales_order_load_after>
            <sales_order_address_load_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>salesOrderAddressAfterLoad</method>
                    </magento_customercustomattributes>
                </observers>
            </sales_order_address_load_after>
            <sales_order_address_collection_load_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>salesOrderAddressCollectionAfterLoad</method>
                    </magento_customercustomattributes>
                </observers>
            </sales_order_address_collection_load_after>
            <sales_order_save_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>salesOrderAfterSave</method>
                    </magento_customercustomattributes>
                </observers>
            </sales_order_save_after>
            <sales_order_address_save_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>salesOrderAddressAfterSave</method>
                    </magento_customercustomattributes>
                </observers>
            </sales_order_address_save_after>
            <magento_customercustomattributes_attribute_before_save>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>enterpriseCustomerAttributeBeforeSave</method>
                    </magento_customercustomattributes>
                </observers>
            </magento_customercustomattributes_attribute_before_save>
            <magento_customercustomattributes_attribute_save>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>enterpriseCustomerAttributeSave</method>
                    </magento_customercustomattributes>
                </observers>
            </magento_customercustomattributes_attribute_save>
            <magento_customercustomattributes_attribute_delete>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>enterpriseCustomerAttributeDelete</method>
                    </magento_customercustomattributes>
                </observers>
            </magento_customercustomattributes_attribute_delete>
            <customer_entity_attribute_save_commit_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>enterpriseCustomerAddressAttributeSave</method>
                    </magento_customercustomattributes>
                </observers>
            </customer_entity_attribute_save_commit_after>
            <customer_entity_attribute_delete_commit_after>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>enterpriseCustomerAddressAttributeDelete</method>
                    </magento_customercustomattributes>
                </observers>
            </customer_entity_attribute_delete_commit_after>
            <core_copy_fieldset_sales_convert_quote_to_order>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>coreCopyFieldsetSalesConvertQuoteToOrder</method>
