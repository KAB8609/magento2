                    </magento_customercustomattributes>
                </observers>
            </core_copy_fieldset_customer_address_to_quote_address>
            <core_copy_fieldset_checkout_onepage_quote_to_customer>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>coreCopyFieldsetCheckoutOnepageQuoteToCustomer</method>
                    </magento_customercustomattributes>
                </observers>
            </core_copy_fieldset_checkout_onepage_quote_to_customer>
            <core_copy_fieldset_sales_convert_quote_address_to_customer_address>
                <observers>
                    <magento_customercustomattributes>
                        <class>Magento_CustomerCustomAttributes_Model_Observer</class>
                        <method>coreCopyFieldsetQuoteAddressToCustomerAddress</method>
                    </magento_customercustomattributes>
                </observers>
            </core_copy_fieldset_sales_convert_quote_address_to_customer_address>
        </events>
    </global>
    <frontend></frontend>
    <adminhtml>
        <events></events>
        <translate>
            <modules>
                <Magento_CustomerCustomAttributes>
                    <files>
                        <default>Magento_CustomerCustomAttributes.csv</default>
                    </files>
                </Magento_CustomerCustomAttributes>
            </modules>
        </translate>
    </adminhtml>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <magento_customercustomattributes before="Magento_Adminhtml">Magento_CustomerCustomAttributes_Adminhtml</magento_customercustomattributes>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>
