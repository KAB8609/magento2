<logging xsi:noNamespaceSchemaLocation="../../../Magento/Logging/etc/logging.xsd"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <event id="adminhtml_customer_attribute" >
        <label translate="true" >Manage Customer Attributes</label >
        <expected_model class="Magento_Customer_Model_Attribute" />
        <handle name="adminhtml_customer_attribute_edit" action="view" />
        <handle name="adminhtml_customer_attribute_save" action="save" />
        <handle name="adminhtml_customer_attribute_delete" action="delete" />
    </event >
    <event id="adminhtml_customer_address_attribute" >
        <label translate="true" >Manage Customer Address Attributes</label >
        <expected_model class="Magento_Customer_Model_Attribute" />
        <handle name="adminhtml_customer_address_attribute_edit" action="view" />
        <handle name="adminhtml_customer_address_attribute_save" action="save" />
        <handle name="adminhtml_customer_address_attribute_delete" action="delete" />
    </event >
</logging >