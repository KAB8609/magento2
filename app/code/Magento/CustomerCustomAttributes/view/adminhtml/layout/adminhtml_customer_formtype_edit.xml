<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="adminhtml_customer_formtype_edit">
        <reference name="head">
            <action method="setCanLoadExtJs">
                <flag>1</flag>
            </action>
        </reference>
        <reference name="content">
            <block class="Magento_CustomerCustomAttributes_Block_Adminhtml_Customer_Formtype_Edit" name="magento_customercustomattributes_formtype_edit"/>
        </reference>
        <reference name="left">
            <block class="Magento_CustomerCustomAttributes_Block_Adminhtml_Customer_Formtype_Edit_Tabs" name="magento_customercustomattributes_formtype_edit_tabs">
                <block class="Magento_CustomerCustomAttributes_Block_Adminhtml_Customer_Formtype_Edit_Tab_General" name="magento_customercustomattributes_formtype_edit_tab_general"/>
                <action method="addTab">
                    <name>magento_customercustomattributes_formtype_edit_tab_general</name>
                    <block>magento_customercustomattributes_formtype_edit_tab_general</block>
                </action>
                <block class="Magento_CustomerCustomAttributes_Block_Adminhtml_Customer_Formtype_Edit_Tab_Tree" name="magento_customercustomattributes_formtype_edit_tab_tree" template="formtype/edit/tab/tree.phtml"/>
                <action method="addTab">
                    <name>magento_customercustomattributes_formtype_edit_tab_tree</name>
                    <block>magento_customercustomattributes_formtype_edit_tab_tree</block>
                </action>
            </block>
        </reference>
    </handle>
</layout>
