<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" label="Multishipping Checkout Address (Any) Form" type="page" parent="checkout_multishipping">
    <update handle="checkout_multishipping"/>
    <reference name="content">
        <block class="Magento_Checkout_Block_Multishipping_Addresses" name="checkout_addresses" template="multishipping/addresses.phtml">
            <block class="Magento_Checkout_Block_Cart_Item_Renderer" as="default" template="multishipping/item/default.phtml"/>
            <block class="Magento_Checkout_Block_Cart_Item_Renderer_Grouped" as="grouped" template="multishipping/item/default.phtml"/>
            <block class="Magento_Checkout_Block_Cart_Item_Renderer_Configurable" as="configurable" template="multishipping/item/default.phtml"/>
        </block>
    </reference>
</layout>
