<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="checkout_multishipping" translate="label" type="page" parent="checkout_cart_index">
        <label>Multishipping Checkout</label>
        <reference name="root">
            <action method="setTemplate">
                <template>1column.phtml</template>
            </action>
        </reference>
        <reference name="content">
            <block class="Magento_Checkout_Block_Multishipping_State" name="checkout_state" template="multishipping/state.phtml"/>
        </reference>
    </handle>
</layout>
