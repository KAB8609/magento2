<config>
    <type name="Magento\GiftCardAccount\Model\Pool">
        <param name="giftCardCodeParams">
            <value>
                <separator>-</separator>
                <charset>
                    <alphanum>ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789</alphanum>
                    <alpha>ABCDEFGHIJKLMNOPQRSTUVWXYZ</alpha>
                    <num>0123456789</num>
                </charset>
            </value>
        </param>
    </type>
    <type name="Magento\Module\Updater\SetupFactory">
        <param name="resourceTypes">
            <value>
                <giftcardaccount_setup>Magento\Sales\Model\Resource\Setup</giftcardaccount_setup>
            </value>
        </param>
    </type>
</config>
