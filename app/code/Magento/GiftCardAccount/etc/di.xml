<config>
    <type name="Magento_GiftCardAccount_Model_History">
        <param name="adminSession">
            <instance type="Magento_Backend_Model_Auth_Session_Proxy" />
        </param>
    </type>
    <type name="Magento_GiftCardAccount_Model_Pool">
        <param name="giftCardCodeParams">
            <value>
                <separator>-</separator>
                <charset>
                    <alphanum>ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789</alphanum>
                    <alpha>ABCDEFGHIJKLMNOPQRSTUVWXYZ</alpha>
                    <num>0123456789</num>
                </charset>
            </value>
        </param>
    </type>
    <type name="Magento_GiftCardAccount_Model_Observer">
        <param name="eventManager">
            <instance type="Magento_Core_Model_Event_Manager_Proxy" />
        </param>
    </type>
</config>
