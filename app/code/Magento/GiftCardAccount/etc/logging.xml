<logging xsi:noNamespaceSchemaLocation="../../../Magento/Logging/etc/logging.xsd"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <event id="magento_giftcardaccount" >
        <label translate="true" >Gift Card Accounts</label >
        <expected_model class="Magento_GiftCardAccount_Model_Giftcardaccount" />
        <handle name="adminhtml_giftcardaccount_edit" action="view" />
        <handle name="adminhtml_giftcardaccount_save" action="save" />
        <handle name="adminhtml_giftcardaccount_delete" action="delete" />
        <handle name="adminhtml_giftcardaccount_massDelete" action="massDelete" />
        <handle name="adminhtml_giftcardaccount_exportCsv" action="export" post_dispatch="postDispatchSimpleSave" />
        <handle name="adminhtml_giftcardaccount_exportMsxml" action="export" post_dispatch="postDispatchSimpleSave" />
    </event >
</logging >