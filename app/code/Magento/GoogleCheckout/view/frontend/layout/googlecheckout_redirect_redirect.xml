<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="googlecheckout_redirect_redirect" translate="label" type="page" parent="default">
        <label>GoogleCheckout Redirect</label>
        <reference name="root">
            <action method="setTemplate">
                <template>2columns-right.phtml</template>
            </action>
        </reference>
        <reference name="content">
            <block class="Magento_GoogleCheckout_Block_Redirect" name="googlecheckout_redirect" template="Magento_Page::redirect.phtml"/>
        </reference>
    </handle>
</layout>
