<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="rss_index_index" translate="label" type="page" parent="default">
        <label>RSS Feeds List</label>
        <reference name="root">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">2columns-right.phtml</argument>
            </action>
        </reference>
        <reference name="content">
            <block class="Magento_Rss_Block_List" name="rss.list" template="list.phtml"/>
        </reference>
    </handle>
</layout>
