<config>
    <modules>
        <Magento_Rss>
            <version>1.6.0.0</version>
            <active>true</active>
             <depends>
                <Magento_Catalog/>
                <Magento_CatalogInventory/>
                <Mage_Sales/>
                <Mage_SalesRule/>
                <Magento_Wishlist/>
             </depends>
        </Magento_Rss>
    </modules>
    <admin>

    </admin>
    <adminhtml>
        <translate>
            <modules>
                <Magento_Rss>
                    <files>
                        <default>Magento_Rss.csv</default>
                    </files>
                </Magento_Rss>
            </modules>
        </translate>
    </adminhtml>
    <frontend>
        <translate>
            <modules>
                <Magento_Rss>
                    <files>
                        <default>Magento_Rss.csv</default>
                    </files>
                </Magento_Rss>
            </modules>
        </translate>
        <secure_url>
            <rss_catalog_review>/rss/catalog/review</rss_catalog_review>
            <rss_order_new>/rss/order/new</rss_order_new>
            <rss_catalog_notifystock>/rss/catalog/notifystock</rss_catalog_notifystock>
        </secure_url>
        <routers>
            <rss>
                <use>standard</use>
                <args>
                    <module>Magento_Rss</module>
                    <frontName>rss</frontName>
                </args>
            </rss>
        </routers>
    </frontend>
</config>
