<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <handle id="install_wizard">
        <block class="Magento_Core_Block_Template" name="root" output="1">
            <action method="setTemplate">
                <template>Magento_Install::page.phtml</template>
            </action>
            <container name="left" as="left" label="Left Column"/>
            <container name="content" as="content" label="Content"/>
        </block>
        <block class="Magento_Install_Block_Db_Main" name="database" as="database"/>
    </handle>
</layout>
