<logging xsi:noNamespaceSchemaLocation="../../../Magento/Logging/etc/logging.xsd"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <title action="preview" translate="true" >Preview</title >
    <title action="publish" translate="true" >Publish</title >
    <event id="magento_versionscms_hierarchy" >
        <label translate="true" >CMS Hierarchy</label >
        <expected_model class="Magento_VersionsCms_Model_Hierarchy_Node" />
        <handle name="adminhtml_cms_hierarchy_index" action="view"
                post_dispatch="Magento_VersionsCms_Model_Observer::postDispatchCmsHierachyView" />
        <handle name="adminhtml_cms_hierarchy_save" action="save" skip_on_back="adminhtml_cms_hierarchy_index " />
    </event >
    <event id="magento_versionscms_page_versioning" >
        <label translate="true" >CMS Versioning</label >
        <handle name="adminhtml_cms_page_massDeleteVersions" action="massDelete" >
            <expected_model class="Magento_VersionsCms_Model_Page_Version" />
        </handle >
        <handle name="adminhtml_cms_page_version_edit" action="view" >
            <expected_model class="Magento_VersionsCms_Model_Page_Version" />
        </handle >
        <handle name="adminhtml_cms_page_version_save" action="save" skip_on_back="adminhtml_cms_page_version_edit " >
            <expected_model class="Magento_VersionsCms_Model_Page_Version" />
        </handle >
        <handle name="adminhtml_cms_page_version_new" action="save" skip_on_back="adminhtml_cms_page_revision_edit " >
            <expected_model class="Magento_VersionsCms_Model_Page_Version" />
        </handle >
        <handle name="adminhtml_cms_page_version_delete" action="delete" >
            <expected_model class="Magento_VersionsCms_Model_Page_Version" />
        </handle >
        <handle name="adminhtml_cms_page_version_massDeleteRevisions" action="massDelete" >
            <expected_model class="Magento_VersionsCms_Model_Page_Revision" />
        </handle >
        <handle name="adminhtml_cms_page_revision_edit" action="view" >
            <expected_model class="Magento_VersionsCms_Model_Page_Revision" />
        </handle >
        <handle name="adminhtml_cms_page_revision_preview" action="preview"
                post_dispatch="Magento_VersionsCms_Model_Observer::postDispatchCmsRevisionPreview" />
        <handle name="adminhtml_cms_page_revision_save" action="save" skip_on_back="adminhtml_cms_page_revision_edit " >
            <expected_model class="Magento_VersionsCms_Model_Page_Revision" />
        </handle >
        <handle name="adminhtml_cms_page_revision_delete" action="delete" >
            <expected_model class="Magento_VersionsCms_Model_Page_Revision" />
        </handle >
        <handle name="adminhtml_cms_page_revision_publish" action="publish"
                skip_on_back="adminhtml_cms_page_revision_edit "
                post_dispatch="Magento_VersionsCms_Model_Observer::postDispatchCmsRevisionPublish" />
    </event >
</logging >