<config>
    <preference for="Magento\Oauth\OauthInterface" type="Magento\Oauth\Oauth"/>
    <preference for="Magento\Oauth\ConsumerInterface" type="Magento\Oauth\Model\Consumer"/>
    <preference for="Magento\Oauth\Nonce\GeneratorInterface" type="Magento\Oauth\Model\Nonce\Generator"/>
    <preference for="Magento\Oauth\Token\ProviderInterface" type="Magento\Oauth\Model\Token\Provider"/>
    <type name="Magento\Oauth\Model\Nonce\Generator">
        <param name="date">
            <instance type="Magento\Core\Model\Date\Proxy"/>
        </param>
    </type>
    <type name="Magento\Oauth\Model\Token\Provider">
        <param name="date">
            <instance type="Magento\Core\Model\Date\Proxy"/>
        </param>
        <param name="dataHelper">
            <instance type="Magento\Oauth\Helper\Data\Proxy"/>
        </param>
    </type>
</config>
