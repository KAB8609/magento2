<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <remove name="js_cookie"/>
    <block class="Magento_Page_Block_Html" name="root" output="1" template="empty.phtml">
        <block class="Magento_Page_Block_Html_Head" name="head" as="head" template="Magento_Oauth::authorize/head-simple.phtml">
            <action method="addCss">
                <argument name="stylesheet" xsi:type="string">Magento_Oauth::css/oauth-simple.css</argument>
            </action>
        </block>
        <container name="content" label="Content"/>
    </block>
</layout>
