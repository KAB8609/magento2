<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <remove name="js_cookie"/>
    <block class="Magento\Page\Block\Html" name="root" output="1" template="empty.phtml">
        <block class="Magento\Page\Block\Html\Head" name="head" as="head" template="Magento_Oauth::authorize/head-simple.phtml">
            <block class="Magento\Page\Block\Html\Head\Css" name="magento-oauth-css-oauth-simple-css">
                <arguments>
                    <argument name="file" xsi:type="string">Magento_Oauth::css/oauth-simple.css</argument>
                </arguments>
            </block>
        </block>
        <container name="content" label="Content"/>
    </block>
</layout>
