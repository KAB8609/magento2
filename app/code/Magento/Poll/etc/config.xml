<config>
    <frontend>
        <routers>
            <poll>
                <use>standard</use>
                <args>
                    <module>Magento_Poll</module>
                    <frontName>poll</frontName>
                </args>
            </poll>
        </routers>
    </frontend>
    <global>
        <resources>
            <poll_setup>
                <setup>
                    <module>Magento_Poll</module>
                </setup>
            </poll_setup>
        </resources>
    </global>
</config>
