<layout version="0.1.0">
    <checkout_multishipping_overview>
        <reference name="head">
            <action method="addJs">
                <file>Magento_Centinel::centinel-authenticate.js</file>
            </action>
        </reference>
        <reference name="checkout.multishipping.overview.items.after">
            <block type="Magento_Centinel_Block_Authentication" name="centinel.frame" template="authentication.phtml">
                <action method="setAuthenticationStartMode">
                    <mode>window</mode>
                </action>
            </block>
        </reference>
    </checkout_multishipping_overview>
</layout>
