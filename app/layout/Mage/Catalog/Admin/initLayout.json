[":catalog_initLayout", {},
    ["#layout",
        [".addMenu",
            ["+menu","#top_menu_catalog",
                [".construct"],
                [".addItem",{
                        "text":"Products and Categories",
                        "id":"catalog_main", 
                        "on":{"click":[["Mage.MenuHandler.makeAction","Mage.MenuHandler",{"loadScript":"true","url":"{$baseUrl}/mage_catalog/index/loadMainPanel"}]]}
                }],
                [".addItem","-"],
                [".addItem",{
                    "text":"New Product",
                    "id":"catalog_new_product", 
                    "on":{"click":[["Mage.MenuHandler.loadPanel","Mage.MenuHandler",{"loadScript":"true","url":"{$baseUrl}/mage_catalog/product/new"}]]}
                }],
                [".addItem",{
                    "text":"New Category",
                    "id":"catalog_new_category", 
                    "on":{"click":[["Mage.MenuHandler.loadPanel","Mage.MenuHandler",{"loadScript":"true","url":"{$baseUrl}/mage_catalog/category/new"}]]}
                }],
                [".addItem","-"],
                [".addItem",{
                    "text":"Attributes",
                    "id":"catalog_attributes", 
                    "on":{"click":[["Mage.MenuHandler.loadPanel","Mage.MenuHandler",{"loadScript":"true","url":"{$baseUrl}/mage_catalog/index/loadAttributesPanel"}]]}
                }]
            ]
        ]
    ],
    ["#top_menu",[".addItem",{"text":"Catalog","cls":"bmenu","menu":"top_menu_catalog"}]]
]