[":catalog_initLayout",
    ["#layout",
        [".addMenu",
            ["+menu","#top_menu_catalog",
                [".construct"],
                [".addItem",
                    {
                        "text":"View Products",
                        "id":"catalog_view_products", 
                        "on":{"click":[["Mage.MenuHandler.loadPanel",{"url":"{$baseUrl}/mage_catalog/","title":"View Products"}]]}
                    }
                ],
                [".addItem",
                    {
                        "text":"View Products Script",
                        "id":"catalog_view_products_script", 
                        "on":{"click":[["Mage.MenuHandler.loadScript",{"url":"{$baseUrl}/mage_catalog/index/addPanel","title":"View Products"}]]}
                    }
                ]
            ]
        ]
    ],
    ["#top_menu",[".addItem",{"text":"Catalog","cls":"bmenu","menu":"top_menu_catalog"}]]
]