<div>
 <?foreach($filters as $_filter):?>
    <?$_arrLinks = $_filter->getLinks()?>
    <?if(!empty($_arrLinks)):?>
        <h3><?=$_filter->getLabel()?></h3>
        <ul>
        <?foreach($_arrLinks as $_link):?>
            <li><a href="<?=$_link['url']?>"><?=$_link['label']?></a></li>
        <?endforeach?>
        </ul>
    <?endif?>
 <?endforeach;?>
</div>