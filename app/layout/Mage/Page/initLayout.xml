<?xml version="1.0"?>
<layoutUpdate>
    <_children>
        <tpl name="root">
            <setViewName module="Mage_Page" view="layout.2column"/>
            <assign var="imagesUrl" value="{$baseSkinUrl}/default/page/images" _process="value"/>
            
            <_children>
                <list name="head" as="head">
                    <_children>
                        <tag name="head.title"><setTagName tagName="title"/><setContents contents="Magenta Commerce"/></tag>
                        <tag_js name="head.js.prototype"><setSrc src="/prototype.js"/></tag_js>
                        <tag_css name="head.css.main"><setHref href="/page/style.css"/></tag_css>
                    </_children>
                </list>
                <list name="top.links" as="topLinks">
                    <_children>
                        <list_link name="top.links.help">
                            <setLink aParams="href=&quot;{$baseModuleUrl}/article/help&quot;" title="Help" _process="aParams"/>
                        </list_link>
                    </_children>
                </list>
                <list name="top.menu" as="topMenu">
                    <_children>
                        <list_link name="top.menu.home">
                            <setLink liParams="id=&quot;nav-home&quot;" aParams="href=&quot;{$baseUrl}&quot;" title="<span>Home</span>" _process="aParams"/>
                        </list_link>
                        <list_link name="top.menu.preventcrime">
                            <setLink liParams="id=&quot;nav-help-prevent&quot;" aParams="href=&quot;{$baseUrl}&quot;" title="<span>Help Prevent Crime</span>" _process="aParams"/>
                        </list_link>
                        <list_link name="top.menu.contact">
                            <setLink liParams="id=&quot;nav-contact&quot;" aParams="href=&quot;{$baseUrl}&quot;" title="<span>Contact Us</span>" _process="aParams"/>
                        </list_link>
                    </_children>
                </list>
                <list name="top.forms" as="topForms"/>
                <list name="left" as="left"/>
                <list name="content" as="content"/>
                <list name="botom.links" as="bottomLinks">
                    <_children>
                        <list_link name="bottom.links.home">
                            <setLink aParams="href=&quot;{$baseUrl}&quot;" title="Home" _process="aParams"/>
                        </list_link>
                        <list_link name="bottom.links.sitemap">
                            <setLink aParams="href=&quot;{$baseModuleUrl}/sitemap&quot;" title="Site Map" _process="aParams"/>
                        </list_link>
                        <list_link name="bottom.links.privacy">
                            <setLink aParams="href=&quot;{$baseModuleUrl}/article/privacy&quot;" title="Privacy Policy" _process="aParams"/>
                        </list_link>
                    </_children>
                </list>
            </_children>
        </tpl>
    </_children>
</layoutUpdate>