[":core_init", {"output":[["root"]]},
    ["+tpl","#root",[".setViewName","Mage_Core","Admin/layout"],
        [".assign","baseUrl","{$baseUrl}"],
        [".assign","baseSkinUrl","{$baseUrl}/skins/default"],

        [".setChild","head",["+list","#head",
            [".append",["+tag","#.title",[".setTagName","title"],[".setContents","MagentaCommerce Admin Office"]]],
            [".append",["+tag_css",[".setHref","/yui-ext/resources/css/ext-all.css",""]]],
            [".append",["+tag_css_theme",[".setHref","/yui-ext/resources/css/ytheme-",""]]],
            [".append",["+tag_css",[".setHref","/styles.css"]]],

            [".append",["+tag_js",[".setSrc","/yui-ext/yui-utilities.js",""]]],
            [".append",["+tag_js",[".setSrc","/yui-ext/ext-yui-adapter.js",""]]],
            [".append",["+tag_js",[".setSrc","/yui-ext/ext-all-debug.js",""]]],
            [".append",["+tag_js",[".setSrc","/magenta/mage.js"]]],
            [".append",["+tag_js",[".setSrc","/magenta/catalog.js"]]],
            [".append",["+tag_js",[".setSrc","/magenta/catalog/product.js"]]],
            [".append",["+tag_js",[".setSrc","/magenta/catalog/category.js"]]],
            [".append",["+tag_js",[".setSrc","/magenta/core.js"]]],
            [".append",["+tag_js",[".setSrc","/magenta/menu.js"]]]            
        ]]
    ]
]