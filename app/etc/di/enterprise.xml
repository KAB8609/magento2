<config>
    <preference for="Enterprise_PageCache_Model_Processor_RestrictionInterface" type="Enterprise_PageCache_Model_Processor_Restriction" />
    <type name="Enterprise_PageCache_Model_Request_Identifier">
        <param name="scopeCode">
            <value type="argument">Mage::PARAM_RUN_CODE</value>
        </param>
    </type>
    <type name="Enterprise_PageCache_Model_Processor">
        <param name="storeManager">
            <instance type="Magento_Core_Model_StoreManager_Proxy" />
        </param>
        <param name="metadata">
            <instance type="Enterprise_PageCache_Model_Metadata_Proxy" />
        </param>
        <param name="requestIdentifier">
            <instance type="Enterprise_PageCache_Model_Request_Identifier_Proxy" />
        </param>
    </type>
    <type name="Enterprise_PageCache_Model_DesignPackage_Rules">
        <param name="designChange">
            <instance type="Magento_Core_Model_Design_Proxy" />
        </param>
        <param name="design">
            <instance type="Magento_Core_Model_View_Design_Proxy" />
        </param>
    </type>
    <type name="Magento_HTTP_Handler_Composite">
        <param name="handlers">
            <value>
                <fpc>
                    <sortOrder>25</sortOrder>
                    <class>Enterprise_PageCache_Model_Http_Handler</class>
                </fpc>
            </value>
        </param>
    </type>
    <type name="Enterprise_Queue_Model_Queue">
        <param name="taskNamePrefix">
            <value type="argument">Enterprise_Queue_Model_Queue::PARAM_TASK_NAME_PREFIX</value>
        </param>
    </type>
</config>
