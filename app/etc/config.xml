<?xml version="1.0"?>
<config>
    <codePool>
        <name>local</name>
        <active>false</active>
    </codePool>

    <codePool>
        <name>community</name>
        <active>false</active>
    </codePool>

    <codePool>
        <name>core</name>
        <active>true</active>
    </codePool>

    <module>
        <name>Mage_Core</name>
        <active>true</active>
        <load>
            <all>
                <resource>
                    <name>dev_setup</name>
                    <connection>
                        <type>mysqli</type>
                        <host>localhost</host>
                        <username>root</username>
                        <password></password>
                        <dbname>magenta</dbname>
                    </connection>
                </resource>
    
                <resource extends="/config/module/load/*/resource[name='dev_setup']">
                    <name>dev_write</name>
                    <connection>
                        <type>mysql</type>
                    </connection>
                </resource>
    
                <resource extends="/config/module/load/*/resource[name='dev_write']">
                    <name>dev_read</name>
                </resource>
    
                <resource extends="/config/module/load/*/resource[name='dev_setup']">
                    <name>core_setup</name>
                </resource>
    
                <resource extends="/config/module/load/*/resource[name='dev_write']">
                    <name>core_write</name>
                </resource>
    
                <resource extends="/config/module/load/*/resource[name='dev_read']">
                    <name>core_read</name>
                </resource>
            </all>
        </load>
    </module>

    <module>
        <name>Mage_Acl</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Page</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Customer</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Catalog</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Cart</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Sales</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Checkout</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Directory</name>
        <active>false</active>
    </module>
</config>