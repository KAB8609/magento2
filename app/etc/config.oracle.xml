<config>
    <global>
        <resource>
            <connection>
                <types>
                    <oracle>
                        <adapter>Magento_DB_Adapter_Oracle</adapter>
                        <compatibleMode>0</compatibleMode>
                    </oracle>
                </types>
            </connection>
        </resource>
    </global>
    <install>
        <databases>
            <oracle>
                <type>oracle</type>
                <min_version>10.2.0.1.0</min_version>
                <extensions>
                    <oci8/>
                </extensions>
            </oracle>
        </databases>
    </install>
</config>
