<config>
    <global>
        <cache>
            <request_processors>
                <ee>
                    <sortOrder>50</sortOrder>
                    <class>Enterprise_PageCache_Model_Processor</class>
                </ee>
            </request_processors>
        </cache>
        <cache_advanced>
            <full_page>
                <backend_options>
                    <cache_dir>full_page_cache</cache_dir>
                </backend_options>
            </full_page>
        </cache_advanced>
        <skip_process_modules_updates>0</skip_process_modules_updates>
        <di>
            <preferences>
                <Enterprise_PageCache_Model_Processor_RestrictionInterface>Enterprise_PageCache_Model_Processor_Restriction</Enterprise_PageCache_Model_Processor_RestrictionInterface>
            </preferences>
            <Enterprise_PageCache_Model_Cache>
                <parameters>
                    <config><instance>Mage_Core_Model_Config_Proxy</instance></config>
                </parameters>
            </Enterprise_PageCache_Model_Cache>
            <Enterprise_PageCache_Model_Request_Identifier>
                <parameters>
                    <scopeCode><argument>Mage::PARAM_RUN_CODE</argument></scopeCode>
                </parameters>
            </Enterprise_PageCache_Model_Request_Identifier>
            <Enterprise_PageCache_Model_Processor>
                <parameters>
                    <storeManager>
                        <instance>Mage_Core_Model_StoreManager_Proxy</instance>
                    </storeManager>
                    <metadata>
                        <instance>Enterprise_PageCache_Model_Metadata_Proxy</instance>
                    </metadata>
                    <requestIdentifier>
                        <instance>Enterprise_PageCache_Model_Request_Identifier_Proxy</instance>
                    </requestIdentifier>
                </parameters>
            </Enterprise_PageCache_Model_Processor>
            <Enterprise_PageCache_Model_DesignPackage_Rules>
                <parameters>
                    <design>
                        <instance>Mage_Core_Model_Design_Proxy</instance>
                    </design>
                    <designPackage>
                        <instance>Mage_Core_Model_Design_Package_Proxy</instance>
                    </designPackage>
                </parameters>
            </Enterprise_PageCache_Model_DesignPackage_Rules>
            <Magento_Http_Handler_Composite>
                <parameters>
                    <handlers>
                        <fpc>
                            <sortOrder>25</sortOrder>
                            <class>Enterprise_PageCache_Model_Http_Handler</class>
                        </fpc>
                    </handlers>
                </parameters>
            </Magento_Http_Handler_Composite>
            <Enterprise_Queue_Helper_Gearman>
                <parameters>
                    <config>
                        <instance>Mage_Core_Model_Config</instance>
                    </config>
                </parameters>
            </Enterprise_Queue_Helper_Gearman>
            <Enterprise_Queue_Model_Queue>
                <parameters>
                    <taskNamePrefix><argument>Enterprise_Queue_Model_Queue::PARAM_TASK_NAME_PREFIX</argument></taskNamePrefix>
                </parameters>
            </Enterprise_Queue_Model_Queue>
        </di>
        <configurators>
            <enterprise_search>Enterprise_Search_Model_ObjectManager_Configurator</enterprise_search>
        </configurators>
    </global>
</config>
