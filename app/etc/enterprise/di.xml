<config>
    <preference for="Magento_FullPageCache_Model_Processor_RestrictionInterface" type="Magento_FullPageCache_Model_Processor_Restriction" />
    <type name="Magento_FullPageCache_Model_Request_Identifier">
        <param name="scopeCode">
            <value type="argument">Mage::PARAM_RUN_CODE</value>
        </param>
    </type>
    <type name="Magento_FullPageCache_Model_Processor">
        <param name="storeManager">
            <instance type="Magento_Core_Model_StoreManager_Proxy" />
        </param>
        <param name="metadata">
            <instance type="Magento_FullPageCache_Model_Metadata_Proxy" />
        </param>
        <param name="requestIdentifier">
            <instance type="Magento_FullPageCache_Model_Request_Identifier_Proxy" />
        </param>
    </type>
    <type name="Magento_FullPageCache_Model_DesignPackage_Rules">
        <param name="designChange">
            <instance type="Magento_Core_Model_Design_Proxy" />
        </param>
        <param name="design">
            <instance type="Magento_Core_Model_View_Design_Proxy" />
        </param>
    </type>
    <type name="Magento\HTTP\Handler\Composite">
        <param name="handlers">
            <value>
                <fpc>
                    <sortOrder>25</sortOrder>
                    <class>Magento_FullPageCache_Model_Http_Handler</class>
                </fpc>
            </value>
        </param>
    </type>
    <type name="Magento_FullPageCache_Model_Http_Handler">
        <param name="requestProcessors">
            <value>
                <ee>
                    <class>Magento_FullPageCache_Model_Processor</class>
                    <sortOrder>50</sortOrder>
                </ee>
            </value>
        </param>
    </type>
    <type name="Magento_Core_Model_Cache_Frontend_Pool">
        <param name="advancedSettings">
            <value>
                <full_page>
                    <backend_options>
                        <cache_dir>full_page_cache</cache_dir>
                    </backend_options>
                </full_page>
            </value>
        </param>
    </type>
</config>
