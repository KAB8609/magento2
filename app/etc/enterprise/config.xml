<config>
    <global>
        <cache>
            <request_processors>
                <ee>
                    <sortOrder>50</sortOrder>
                    <class>Enterprise_PageCache_Model_Processor</class>
                </ee>
            </request_processors>
        </cache>
        <cache_advanced>
            <full_page>
                <backend_options>
                    <cache_dir>full_page_cache</cache_dir>
                </backend_options>
            </full_page>
        </cache_advanced>
        <skip_process_modules_updates>0</skip_process_modules_updates>
        <configurators>
            <enterprise_search>Enterprise_Search_Model_ObjectManager_Configurator</enterprise_search>
        </configurators>
    </global>
</config>
