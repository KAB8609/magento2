<?xml version="1.0"?>
<globalConfig>
    <codePool>
        <name>local</name>
        <active>false</active>
    </codePool>

    <codePool>
        <name>community</name>
        <active>false</active>
    </codePool>

    <codePool>
        <name>core</name>
        <active>true</active>
    </codePool>

    <module>
        <name>Mage_Core</name>
        <active>true</active>
        <load area="all">
            <resource>
                <name>dev_setup</name>
                <type>mysqli</type>
                <host>localhost</host>
                <username>root</username>
                <password></password>
                <dbname>magenta</dbname>
            </resource>

            <resource preload="../resource[name=dev_setup]">
                <name>dev_write</name>
                <type>mysql</type>
            </resource>

            <resource preload="../resource[name=dev_write]">
                <name>dev_read</name>
            </resource>

            <resource preload="../resource[name=dev_setup]">
                <name>core_setup</name>
            </resource>

            <resource preload="../resource[name=dev_write]">
                <name>core_write</name>
            </resource>

            <resource preload="../resource[name=dev_read]">
                <name>core_read</name>
            </resource>
         </load>
    </module>

    <module>
        <name>Mage_Acl</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Page</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Customer</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Catalog</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Cart</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Sales</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Checkout</name>
        <active>true</active>
    </module>

    <module>
        <name>Mage_Directory</name>
        <active>false</active>
    </module>
</globalConfig>