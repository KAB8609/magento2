<phpunit bootstrap="framework/bootstrap.php">
    <testsuites>
        <testsuite name="All Tests">
            <directory suffix="Test.php">testsuite</directory>
        </testsuite>
    </testsuites>
    <php>
        <const name="SELENIUM_TESTS_INSTALLATION" value="enabled"/>
    </php>
    <logging>
        <log type="tap" target="tests-functional.tap"/>
        <log type="junit" target="tests-functional.xml"/>
    </logging>
</phpunit>
