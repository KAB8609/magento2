<renders>
    <default>
        <type>*/*</type>
        <model>api2/renderer_json</model>
    </default>
    <application_json>
        <type>application/json</type>
        <model>api2/renderer_json</model>
    </application_json>

    <text_plain>
        <type>text/plain</type>
        <model>api2/renderer_query</model>
    </text_plain>

    <text_xml>
        <type>text/xml</type>
        <model>api2/renderer_xml</model>
    </text_xml>
    <application_xml>
        <type>application/xml</type>
        <model>api2/renderer_xml</model>
    </application_xml>
    <application_xhtml_xml>
        <type>application/xhtml+xml</type>
        <model>api2/renderer_xml</model>
    </application_xhtml_xml>

    <text_html>
        <type>text/html</type>
        <model>api2/renderer_html</model>
    </text_html>
    <application_html>
        <type>application/html</type>
        <model>api2/renderer_html</model>
    </application_html>
</renders>
