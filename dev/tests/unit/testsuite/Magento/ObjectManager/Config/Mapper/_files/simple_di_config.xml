<config>
    <!-- Comment within root node -->
    <preference for="Mage_Core_Model_Db_UpdaterInterface" type="Mage_Core_Model_Db_Updater" />
    <preference for="Mage_Core_Model_AppInterface" type="Mage_Core_Model_App_Proxy" />
    <type name="Mage_Core_Model_App_State">
        <param name="mode">
            <argument name="Mage::PARAM_MODE" />
        </param>
    </type>
    <type name="Mage_Core_Model_Config_Modules">
        <!-- Comment within type node -->
        <param name="storage">
            <!-- Comment within param node -->
            <instance type="Mage_Core_Model_Config_Storage_Modules" shared="false" />
        </param>
        <plugin name="simple_modules_plugin" type="Mage_Core_Model_Config_Modules_Plugin" disabled="true" sortOrder="10" />
        <plugin name="simple_modules_plugin_advanced" type="Mage_Core_Model_Config_Modules_PluginAdvanced" />
        <plugin name="overridden_plugin" sortOrder="30" disabled="true" />
    </type>
    <type name="Magento_Http_Handler_Composite" shared="false">
        <param name="factory">
            <instance type="Magento_Http_HandlerFactory" />
        </param>
        <param name="handlers">
            <value>
                <!-- Comment within value node -->
                <custom_handler>
                    <sortOrder>25</sortOrder>
                    <class>Custom_Cache_Model_Http_Handler</class>
                </custom_handler>
                <other_handler>
                    <!-- Comment within child node of the value node that represents an assoc array -->
                    <sortOrder>10</sortOrder>
                    <class><![CDATA[Other_Cache_Model_Http_Handler]]></class>
                </other_handler>
            </value>
        </param>
    </type>
    <type name="Varien_Data_Collection_Db_FetchStrategy_Cache">
        <param name="cacheIdPrefix"><value><![CDATA[collection_]]></value></param>
        <param name="cacheLifetime"><value>86400</value></param>
    </type>
    <virtualType name="customCacheInstance" type="Mage_Core_Model_Cache" shared="true" />
    <virtualType name="customOverriddenInstance" shared="false" />
</config>
