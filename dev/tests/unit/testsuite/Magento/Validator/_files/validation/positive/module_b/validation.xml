<validation>
    <entity name="test_entity">
        <rules>
            <rule name="test_rule_constraint_2">
                <property_constraints>
                    <property name="constraint_field">
                        <constraint class="Magento_Validator_Test_NotEmpty"/>
                    </property>
                </property_constraints>
            </rule>
        </rules>
        <groups>
            <group name="test_group_b">
                <uses>
                    <use rule="test_rule_constraint_2"/>
                </uses>
            </group>
        </groups>
    </entity>
    <entity name="test_entity_b">
        <rules>
            <rule name="test_rule">
                <entity_constraints>
                    <constraint class="Magento_Validator_Test_True"/>
                </entity_constraints>
            </rule>
        </rules>
        <groups>
            <group name="test_group_c" builder="CustomBuilderClass">
                <uses>
                    <use rule="test_rule"/>
                </uses>
            </group>
        </groups>
    </entity>
</validation>
