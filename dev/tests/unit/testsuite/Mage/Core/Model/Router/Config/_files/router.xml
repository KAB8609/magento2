<config>
    <router id="backend" instance="Mage_Core_Controller_Router_Admin" frontName="backend" disabled="true">
        <route id="adminhtml" frontName="admin">
            <module name="Mage_Adminhtml" />
            <module name="Mage_Catalog" before="Mage_Adminhtml" />
            <module name="Mage_Wishlist" before="Mage_Adminhtml" />
            <module name="Mage_Sales" before="Mage_Catalog" />
            <module name="Mage_GiftCard" />
            <module name="Mage_GiftCardAccount" after="Mage_GiftCard" />
        </route>
        <route id="customer" frontName="customer">
            <module name="Mage_Customer" before="Mage_Wishlist" />
        </route>
        <route id="wishlist" frontName="wishlist">
            <module name="Mage_Wishlist" />
        </route>
    </router>
    <router id="front" instance="Mage_Core_Controller_Router_Base" frontName="" disabled="" />
</config>