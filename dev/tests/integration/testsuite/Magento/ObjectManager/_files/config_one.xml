<config>
    <preference for="Mage_Core_Model_Db_UpdaterInterface" type="Mage_Core_Model_Db_Updater" />
    <type name="Mage_Core_Model_Config_Loader_Local">
        <param name="customFile">
            <argument name="Mage::PARAM_CUSTOM_LOCAL_FILE" />
        </param>
        <param name="customConfig">
            <instance shared="true" type="Mage_Core_Model_Config_Storage_Modules" />
        </param>
        <param name="customValue">
            <value>someValue</value>
        </param>
        <param name="customComplexValue">
            <value transalte="one,two" module="Some_Module">
                <one>one</one>
                <two>two</two>
            </value>
        </param>
        <plugin name="one" type="somePluginOne"/>
        <plugin name="two" type="somePluginTwo" sortOrder="15"/>
    </type>
    <virtualType name="Mage_Core_Model_Config_Loader_LocalVirtual" type="Mage_Core_Model_Config_Loader_Local" shared="false">
        <param name="customFile">
            <argument name="Mage::PARAM_CUSTOM_LOCAL_FILE" />
        </param>
        <param name="customConfig">
            <instance type="Mage_Core_Model_Config_Storage_Modules" />
        </param>
        <param name="customValue">
            <value>someValue</value>
        </param>
        <param name="customComplexValue">
            <value transalte="one,two" module="Some_Module">
                <one>one</one>
                <two>one</two>
            </value>
        </param>
        <plugin name="one" type="somePluginOne" sortOrder="10" />
        <plugin name="two" type="somePluginTwo"/>
    </virtualType>
</config>
