    </block>
    <reference name="head">
        <block type="Mage_Page_Block_Html_Head_Script" name="">
            <arguments>
                <file>1</file>
            </arguments>
        </block>
    </reference>
    <reference name="custom.not_safe">%A</reference>
</layout>
