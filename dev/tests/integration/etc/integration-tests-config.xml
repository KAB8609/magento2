<config>
    <global>
        <events>
            <controller_front_init_before>
                <observers>
                    <integration_tests>
                        <class>Magento_Test_Event_Magento</class>
                        <method>initFrontControllerBefore</method>
                    </integration_tests>
                </observers>
            </controller_front_init_before>
        </events>
        <di>
            <aliases>
                <Mage_Core_Model_Cookie>Magento_Test_Cookie</Mage_Core_Model_Cookie>
                <Mage_Core_Controller_Request_Http>Magento_Test_Request</Mage_Core_Controller_Request_Http>
                <Mage_Core_Controller_Response_Http>Magento_Test_Response</Mage_Core_Controller_Response_Http>
            </aliases>
        </di>
        <dev>
            <design_fallback>
                <allow_map_update>0</allow_map_update>
            </design_fallback>
        </dev>
    </global>
</config>
