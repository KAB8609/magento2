<config>
    <service class="Mage_TestModule3_Service_ErrorInterfaceV1" baseUrl="/V1/errortest">
        <rest-route httpMethod="GET" method="success">/success</rest-route>
        <rest-route httpMethod="GET" method="resourceNotFoundException">/notfound</rest-route>
        <rest-route httpMethod="GET" method="serviceException">/serviceexception</rest-route>
        <rest-route httpMethod="GET" method="parameterizedServiceException">/parameterizedserviceexception</rest-route>
        <rest-route httpMethod="GET" method="authorizationException">/unauthorized</rest-route>
        <rest-route httpMethod="GET" method="otherException">/otherexception</rest-route>
    </service>
</config>
